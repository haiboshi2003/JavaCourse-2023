{"ast":null,"code":"import { ref, computed, onMounted } from 'vue';\nimport axios from \"../api/request\";\nimport { message } from 'ant-design-vue';\nimport * as echarts from 'echarts';\nexport default {\n  __name: 'searchtitle',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const dto = ref({\n      current: 1,\n      size: 5,\n      id: '',\n      name: ''\n    });\n    const data = ref([]);\n    const data2 = ref([]);\n    const searchid = ref('');\n    function change(data) {\n      for (let i = 0; i < data.value.length; i++) {\n        if (data.value[i].riskLevel === 0) data.value[i].riskLevel = '谨慎';else if (data.value[i].riskLevel === 1) data.value[i].riskLevel = '稳健';else if (data.value[i].riskLevel === 2) data.value[i].riskLevel = '平衡';else if (data.value[i].riskLevel === 3) data.value[i].riskLevel = '进取';else if (data.value[i].riskLevel === 4) data.value[i].riskLevel = '激进';\n        if (data.value[i].status === 0) data.value[i].status = '开放申购';else if (data.value[i].status === 1) data.value[i].status = '不可申购';\n        if (data.value[i].type === 0) data.value[i].type = '股票型';else if (data.value[i].type === 1) data.value[i].type = '债券型';else if (data.value[i].type === 2) data.value[i].type = '混合型';else if (data.value[i].type === 3) data.value[i].type = '货币型';else if (data.value[i].type === 4) data.value[i].type = '其它';\n      }\n      return data;\n    }\n    async function getdata() {\n      try {\n        const resp = await axios.get('/product', {\n          params: {\n            current: dto.value.current,\n            size: dto.value.size\n          }\n        });\n        //提出响应数据对象\n        data2.value = resp.data;\n        data.value = resp.data;\n        // 处理数据\n        console.log('获取成功');\n        console.log(data.value);\n        change(data);\n      } catch (error) {\n        // 处理错误\n        console.error(error);\n      }\n    }\n    getdata();\n    const products = computed(() => {\n      return data.value || [];\n    });\n    const pagination = computed(() => {\n      return {\n        current: dto.value.current,\n        // 当前页\n        pageSize: dto.value.size,\n        // 页大小\n        total: products.length,\n        // 总记录数\n        showSizeChanger: true,\n        // 显示页大小的下拉列表\n        pageSizeOptions: [\"3\", \"5\", \"10\", \"50\", \"100\"] // 自定义下拉列表内容\n      };\n    });\n    // 在页号或页大小改变时调用\n    function tableChange(pagination) {\n      console.log(pagination);\n      dto.value.current = pagination.current ?? 1;\n      dto.value.size = pagination.pageSize ?? 5;\n      // run(dto.value)\n    }\n\n    //搜索功能\n    async function searchbyid() {\n      console.log(searchid);\n      console.log(typeof searchid);\n      await getdata();\n      if (searchid == '') {\n        data.value = data2.value;\n        change(data);\n      } else {\n        data.value = data2.value.filter(number => {\n          return number.id.startsWith(searchid.value);\n        });\n        change(data);\n      }\n    }\n\n    // >>>>>>>>>>>>>> 删除功能开始\n    function onDelete(myid) {\n      console.log(myid);\n      axios.delete('/product', {\n        data: myid\n      }).then(response => {\n        console.log(response.data);\n        searchbyid(); //                   查询请求 查询响应\n        if (response.data.success) message.success('删除成功');else message.error('删除失败');\n      }).catch(error => {\n        console.error(error);\n      }); // 删除请求 删除响应\n    }\n\n    // >>>>>>>>>>>>>> 删除功能结束\n\n    // >>>>>>>>>>>>>> 修改功能开始\n\n    const newdata = ref({\n      id: '',\n      name: '',\n      shortName: '',\n      company: '',\n      managementFee: '',\n      manager: '',\n      nav: '',\n      redemptionFee: '',\n      subscriptionFee: '',\n      riskLevel: '',\n      salesFee: '',\n      startDate: '',\n      status: '',\n      totalShare: '',\n      type: '',\n      custodianFee: ''\n    });\n    const open = ref(false);\n    const showModal = record => {\n      console.log('record', record);\n      open.value = true;\n      newdata.value = record;\n      if (newdata.value.riskLevel == '谨慎') newdata.value.riskLevel = 0;else if (newdata.value.riskLevel == '稳健') newdata.value.riskLevel = 1;else if (newdata.value.riskLevel == '平衡') newdata.value.riskLevel = 2;else if (newdata.value.riskLevel == '进取') newdata.value.riskLevel = 3;else if (newdata.value.riskLevel == '激进') newdata.value.riskLevel = 4;\n      if (newdata.value.type == '股票型') newdata.value.type = 0;else if (newdata.value.type == '债券型') newdata.value.type = 1;else if (newdata.value.type == '混合型') newdata.value.type = 2;else if (newdata.value.type == '货币型') newdata.value.type = 3;else if (newdata.value.type == 'ETF联接') newdata.value.type = 4;else if (newdata.value.type == '其它') newdata.value.type = 5;\n      if (newdata.value.status == '开放申购') newdata.value.status = 0;else if (newdata.value.status == '不可申购') newdata.value.status = 1;\n    };\n    async function handleOk() {\n      await modifyproduct();\n      console.log('newdata', newdata.value);\n      open.value = false;\n    }\n    ;\n    function modifyproduct() {\n      newdata.value.custodianFee = parseFloat(newdata.value.custodianFee);\n      newdata.value.nav = parseFloat(newdata.value.nav);\n      newdata.value.redemptionFee = parseFloat(newdata.value.redemptionFee);\n      newdata.value.salesFee = parseFloat(newdata.value.salesFee);\n      newdata.value.subscriptionFee = parseFloat(newdata.value.subscriptionFee);\n      newdata.value.totalShare = parseFloat(newdata.value.totalShare);\n      newdata.value.managementFee = parseFloat(newdata.value.managementFee);\n      console.log(newdata.value);\n      axios.post('/product', newdata.value).then(response => {\n        const resp = response.data;\n        console.log(resp);\n        if (resp.success) {\n          searchbyid();\n          message.success('修改产品成功', 5);\n        } else {\n          message.info(`修改产品失败,${resp.message}`);\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n\n    // >>>>>>>>>>>>>> 修改功能结束\n\n    // >>>>>>>>>>>>>添加净值功能开始\n\n    const addNav = ref({\n      id: '',\n      date: \"\",\n      nav: null\n    });\n    const open2 = ref(false);\n\n    //控制展开窗口\n    const showModa2 = id => {\n      open2.value = true;\n      addNav.value.id = id;\n    };\n    //控制关闭窗口\n    async function handleOk2() {\n      await navAdd();\n      open2.value = false;\n    }\n    ;\n    function navAdd() {\n      addNav.value.nav = parseFloat(addNav.value.nav);\n      axios.put('/product/nav', addNav.value).then(response => {\n        const resp = response.data;\n        console.log(resp);\n        if (resp.success) {\n          searchbyid();\n          message.success('添加成功', 5);\n        } else {\n          message.info(`添加失败,${resp.message}`);\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n\n    // >>>>>>>>>添加产品净值结束\n\n    const columns = ref([{\n      title: '产品代码',\n      dataIndex: 'id',\n      fixed: 'left',\n      // width: 100,\n      key: 'id'\n    }, {\n      title: '产品名称',\n      dataIndex: 'name',\n      fixed: 'left',\n      // width: 100,\n      key: 'name'\n    }, {\n      title: '产品简称',\n      dataIndex: 'shortName',\n      key: '1'\n    }, {\n      title: '发行公司',\n      dataIndex: 'company',\n      key: '2'\n    }, {\n      title: '管理费率',\n      dataIndex: 'managementFee',\n      key: '3'\n    }, {\n      title: '基金经理',\n      dataIndex: 'manager',\n      key: '4'\n    }, {\n      title: '产品净值',\n      dataIndex: 'nav',\n      key: '5'\n    }, {\n      title: '赎回费率',\n      dataIndex: 'redemptionFee',\n      key: '6'\n    }, {\n      title: '申购费率',\n      dataIndex: 'subscriptionFee',\n      key: '7'\n    }, {\n      title: '风险等级',\n      dataIndex: 'riskLevel',\n      key: '8'\n    }, {\n      title: '销售管理费率',\n      dataIndex: 'salesFee',\n      key: '9'\n    }, {\n      title: '成立日期',\n      dataIndex: 'startDate',\n      key: '10'\n    }, {\n      title: '产品状态',\n      dataIndex: 'status',\n      key: '11'\n    }, {\n      title: '产品总份额',\n      dataIndex: 'totalShare',\n      key: '12'\n    }, {\n      title: '产品类型',\n      dataIndex: 'type',\n      key: '13'\n    }, {\n      title: '操作',\n      dataIndex: 'operation',\n      key: '14',\n      fixed: 'right'\n      // width: 100,\n    }, {\n      title: '净值操作',\n      dataIndex: 'navoperation',\n      key: '15',\n      fixed: 'right'\n      // width: 100,\n    }]);\n    const __returned__ = {\n      dto,\n      data,\n      data2,\n      searchid,\n      change,\n      getdata,\n      products,\n      pagination,\n      tableChange,\n      searchbyid,\n      onDelete,\n      newdata,\n      open,\n      showModal,\n      handleOk,\n      modifyproduct,\n      addNav,\n      open2,\n      showModa2,\n      handleOk2,\n      navAdd,\n      columns,\n      ref,\n      computed,\n      onMounted,\n      get axios() {\n        return axios;\n      },\n      get message() {\n        return message;\n      },\n      get echarts() {\n        return echarts;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","axios","message","echarts","dto","current","size","id","name","data","data2","searchid","change","i","value","length","riskLevel","status","type","getdata","resp","get","params","console","log","error","products","pagination","pageSize","total","showSizeChanger","pageSizeOptions","tableChange","searchbyid","filter","number","startsWith","onDelete","myid","delete","then","response","success","catch","newdata","shortName","company","managementFee","manager","nav","redemptionFee","subscriptionFee","salesFee","startDate","totalShare","custodianFee","open","showModal","record","handleOk","modifyproduct","parseFloat","post","info","addNav","date","open2","showModa2","handleOk2","navAdd","put","columns","title","dataIndex","fixed","key"],"sources":["H:/GitHub-Repository/hengsheng/hello-world/src/components/searchtitle.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <a-row>\r\n      <a-col :span=\"16\"></a-col>\r\n      <a-col :span=\"5\">\r\n        <a-input\r\n          v-model:value=\"searchid\"\r\n          placeholder=\"输入产品代码\"\r\n          size=\"large\"\r\n          style=\"width: 340px\"\r\n        ></a-input>\r\n      </a-col>\r\n      <a-col :span=\"1\"></a-col>\r\n\r\n      <a-col :span=\"2\">\r\n        <a-button @click=\"searchbyid\" type=\"primary\" size=\"large\"\r\n          >搜索</a-button\r\n        >\r\n      </a-col>\r\n    </a-row>\r\n    <hr />\r\n\r\n    <div class=\"tb-container\">\r\n      <a-table\r\n        :columns=\"columns\"\r\n        :dataSource=\"products\"\r\n        rowKey=\"id\"\r\n        :pagination=\"pagination\"\r\n        :scroll=\"{ y: 450 }\"\r\n        @change=\"tableChange\"\r\n      >\r\n        <template #bodyCell=\"{ column, record }\">\r\n          <template v-if=\"column.dataIndex === 'operation'\">\r\n            <a-popconfirm\r\n              placement=\"left\"\r\n              title=\"确认要修改该产品吗?\"\r\n              ok-text=\"确定\"\r\n              cancel-text=\"取消\"\r\n              @confirm=\"showModal(record)\"\r\n            >\r\n              <a>修改</a>\r\n            </a-popconfirm>\r\n            <a-divider type=\"vertical\"></a-divider>\r\n\r\n            <a-popconfirm\r\n              placement=\"left\"\r\n              title=\"确认要删除该产品吗?\"\r\n              ok-text=\"确定\"\r\n              cancel-text=\"取消\"\r\n              @confirm=\"onDelete(record.id)\"\r\n            >\r\n              <a>删除</a>\r\n            </a-popconfirm>\r\n          </template>\r\n\r\n          <template v-if=\"column.dataIndex === 'navoperation'\">\r\n            <a @click=\"showModa2(record.id)\">添加</a>\r\n          </template>\r\n        </template>\r\n      </a-table>\r\n    </div>\r\n\r\n    <!-- 修改部分 -->\r\n    <div>\r\n      <a-modal\r\n        v-model:open=\"open\"\r\n        title=\"修改产品信息\"\r\n        ok-text=\"确定修改\"\r\n        cancel-text=\"取消\"\r\n        width=\"1200px\"\r\n        @ok=\"handleOk\"\r\n      >\r\n        <div>\r\n          <br />\r\n          <a-row>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <label for=\"id\">产品代码：</label>\r\n              <a-input v-model:value=\"newdata.id\" id=\"id\" :disabled=\"true\" />\r\n            </a-col>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <label for=\"100\">产品名称：</label>\r\n              <a-input v-model:value=\"newdata.name\" id=\"100\" />\r\n            </a-col>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <label for=\"101\">产品简称：</label>\r\n              <a-input v-model:value=\"newdata.shortName\" id=\"101\" />\r\n            </a-col>\r\n          </a-row>\r\n\r\n          <br />\r\n          <br />\r\n\r\n          <a-row>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <label for=\"102\">发行公司：</label>\r\n\r\n              <a-input v-model:value=\"newdata.company\" id=\"102\" />\r\n            </a-col>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <label for=\"103\">管理费率：</label>\r\n\r\n              <a-input v-model:value=\"newdata.managementFee\" id=\"103\" />\r\n              <span class=\"hint-text\">请输入小于1的数作为管理费率</span>\r\n            </a-col>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <label for=\"104\">基金经理：</label>\r\n\r\n              <a-input v-model:value=\"newdata.manager\" id=\"104\" />\r\n            </a-col>\r\n          </a-row>\r\n\r\n          <br />\r\n          <br />\r\n\r\n          <a-row>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <label for=\"105\">产品净值：</label>\r\n\r\n              <a-input v-model:value=\"newdata.nav\" id=\"105\" />\r\n            </a-col>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <label for=\"106\">赎回费率：</label>\r\n\r\n              <a-input v-model:value=\"newdata.redemptionFee\" id=\"106\" />\r\n              <span class=\"hint-text\">请输入小于1的数作为赎回费率</span>\r\n            </a-col>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <label for=\"107\">申购费率：</label>\r\n\r\n              <a-input v-model:value=\"newdata.subscriptionFee\" id=\"107\" />\r\n              <span class=\"hint-text\">请输入小于1的数作为申购费率</span>\r\n            </a-col>\r\n          </a-row>\r\n\r\n          <br />\r\n          <br />\r\n          <a-row>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"7\">\r\n              <label for=\"riskLevel\">风险等级：</label>\r\n              <a-select\r\n                id=\"riskLevel\"\r\n                ref=\"select\"\r\n                v-model:value=\"newdata.riskLevel\"\r\n                style=\"width: 250px\"\r\n              >\r\n                <a-select-option :value=\"Number(0)\">谨慎</a-select-option>\r\n                <a-select-option :value=\"Number(1)\">稳健</a-select-option>\r\n                <a-select-option :value=\"Number(2)\">平稳</a-select-option>\r\n                <a-select-option :value=\"Number(3)\">进取</a-select-option>\r\n                <a-select-option :value=\"Number(4)\">激进</a-select-option>\r\n              </a-select>\r\n            </a-col>\r\n          </a-row>\r\n\r\n          <br />\r\n          <br />\r\n\r\n          <a-row>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <label for=\"108\">销售管理费率：</label>\r\n\r\n              <a-input v-model:value=\"newdata.salesFee\" id=\"108\" />\r\n              <span class=\"hint-text\">请输入小于1的数作为销售管理费率</span>\r\n            </a-col>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <label for=\"109\">日期：</label>\r\n\r\n              <a-date-picker\r\n                :value-format=\"'YYYY-MM-DD'\"\r\n                id=\"109\"\r\n                v-model:value=\"newdata.startDate\"\r\n              />\r\n            </a-col>\r\n          </a-row>\r\n\r\n          <br />\r\n          <br />\r\n\r\n          <a-row>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <label for=\"status\">产品状态：</label>\r\n              <a-select\r\n                id=\"status\"\r\n                ref=\"select\"\r\n                v-model:value=\"newdata.status\"\r\n                style=\"width: 250px\"\r\n              >\r\n                <a-select-option :value=\"Number(0)\">开放申购</a-select-option>\r\n                <a-select-option :value=\"Number(1)\">不可申购</a-select-option>\r\n              </a-select>\r\n            </a-col>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <label for=\"type\">产品类型：</label>\r\n              <a-select\r\n                ref=\"select\"\r\n                id=\"type\"\r\n                v-model:value=\"newdata.type\"\r\n                style=\"width: 250px\"\r\n              >\r\n                <a-select-option :value=\"Number('0')\">股票型</a-select-option>\r\n                <a-select-option :value=\"Number('1')\">债券型</a-select-option>\r\n                <a-select-option :value=\"Number('2')\">混合型</a-select-option>\r\n                <a-select-option :value=\"Number('3')\">货币型</a-select-option>\r\n                <a-select-option :value=\"Number('4')\">ETF联接</a-select-option>\r\n                <a-select-option :value=\"Number('5')\">其他</a-select-option>\r\n              </a-select>\r\n            </a-col>\r\n          </a-row>\r\n          <br />\r\n          <br />\r\n          <br />\r\n\r\n          <a-row>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <label for=\"110\">产品总份额：</label>\r\n\r\n              <a-input v-model:value=\"newdata.totalShare\" id=\"110\" />\r\n            </a-col>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <label for=\"111\">托管费率：</label>\r\n\r\n              <a-input v-model:value=\"newdata.custodianFee\" id=\"111\" />\r\n\r\n              <span class=\"hint-text\">请输入小于1的数作为托管费率</span>\r\n            </a-col>\r\n          </a-row>\r\n\r\n          <br />\r\n          <br />\r\n        </div>\r\n      </a-modal>\r\n    </div>\r\n    <!-- 查看产品净值 -->\r\n    <div>\r\n      <a-modal\r\n        v-model:open=\"open2\"\r\n        title=\"添加产品净值\"\r\n        ok-text=\"确定\"\r\n        cancel-text=\"取消\"\r\n        width=\"1200px\"\r\n        @ok=\"handleOk2\"\r\n      >\r\n        <div>\r\n          <br />\r\n          <a-row>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <a-input\r\n                v-model:value=\"addNav.id\"\r\n                placeholder=\"产品代码\"\r\n                :disabled=\"true\"\r\n              />\r\n            </a-col>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <a-date-picker\r\n                :value-format=\"'YYYY-MM-DD'\"\r\n                placeholder=\"请选择时间\"\r\n                v-model:value=\"addNav.date\"\r\n              />\r\n            </a-col>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <a-input v-model:value=\"addNav.nav\" placeholder=\"产品净值\" />\r\n            </a-col>\r\n          </a-row>\r\n        </div>\r\n      </a-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup lang=\"js\">\r\n\r\n\r\nimport { ref, computed, onMounted} from 'vue';\r\nimport axios from \"../api/request\";\r\nimport { message } from 'ant-design-vue';\r\n\r\n\r\nconst dto = ref({ current: 1, size: 5, id: '',name : '' })\r\nconst data = ref([]);\r\nconst data2 = ref([]);\r\nconst searchid = ref('')\r\n\r\nfunction change(data) {\r\n\r\n  for (let i = 0; i < data.value.length; i++) {\r\n    if (data.value[i].riskLevel === 0)\r\n      data.value[i].riskLevel = '谨慎'\r\n    else if (data.value[i].riskLevel === 1)\r\n      data.value[i].riskLevel = '稳健'\r\n    else if (data.value[i].riskLevel === 2)\r\n      data.value[i].riskLevel = '平衡'\r\n    else if (data.value[i].riskLevel === 3)\r\n      data.value[i].riskLevel = '进取'\r\n    else if (data.value[i].riskLevel === 4)\r\n      data.value[i].riskLevel = '激进'\r\n\r\n    if (data.value[i].status === 0)\r\n      data.value[i].status = '开放申购'\r\n    else if (data.value[i].status === 1)\r\n      data.value[i].status = '不可申购'\r\n\r\n    if (data.value[i].type === 0)\r\n      data.value[i].type = '股票型'\r\n    else if (data.value[i].type === 1)\r\n      data.value[i].type = '债券型'\r\n    else if (data.value[i].type === 2)\r\n      data.value[i].type = '混合型'\r\n    else if (data.value[i].type === 3)\r\n      data.value[i].type = '货币型'\r\n    else if (data.value[i].type === 4)\r\n      data.value[i].type = '其它'\r\n  }\r\n  return data\r\n\r\n}\r\n\r\nasync function getdata() {\r\n  try {\r\n\r\n    const resp = await axios.get('/product', {\r\n      params: {\r\n        current: dto.value.current,\r\n        size: dto.value.size\r\n      }\r\n    })\r\n    //提出响应数据对象\r\n    data2.value = resp.data;\r\n    data.value = resp.data;\r\n    // 处理数据\r\n    console.log('获取成功');\r\n    console.log(data.value);\r\n    change(data);\r\n\r\n\r\n  } catch (error) {\r\n    // 处理错误\r\n    console.error(error);\r\n\r\n\r\n  }\r\n}\r\n\r\ngetdata()\r\n\r\nconst products = computed(()=>{\r\n  return data.value|| []\r\n})\r\n\r\n\r\nconst pagination = computed(()=>{\r\n  return {\r\n    current: dto.value.current, // 当前页\r\n    pageSize: dto.value.size, // 页大小\r\n    total: products.length,       // 总记录数\r\n    showSizeChanger: true,    // 显示页大小的下拉列表\r\n    pageSizeOptions: [\"3\",\"5\",\"10\",\"50\",\"100\"] // 自定义下拉列表内容\r\n  }\r\n})\r\n// 在页号或页大小改变时调用\r\nfunction tableChange(pagination) {\r\n  console.log(pagination)\r\n  dto.value.current = pagination.current ?? 1\r\n  dto.value.size = pagination.pageSize ?? 5\r\n  // run(dto.value)\r\n}\r\n\r\n\r\n//搜索功能\r\nasync function searchbyid() {\r\n  console.log(searchid)\r\n  console.log(typeof (searchid));\r\n  await getdata()\r\n  if (searchid == '') {\r\n    data.value = data2.value;\r\n    change(data);\r\n\r\n  }\r\n  else {\r\n    data.value = data2.value.filter((number) => {\r\n      return number.id.startsWith(searchid.value);\r\n    });\r\n    change(data);\r\n\r\n  }\r\n\r\n}\r\n\r\n// >>>>>>>>>>>>>> 删除功能开始\r\nfunction onDelete(myid) {\r\n  console.log(myid);\r\n\r\n  axios.delete('/product', {data:myid})\r\n  .then(response => {\r\n    console.log(response.data);\r\n\r\n    searchbyid()       //                   查询请求 查询响应\r\n    if (response.data.success)\r\n      message.success('删除成功')\r\n    else\r\n      message.error('删除失败')\r\n\r\n\r\n  })\r\n  .catch(error => {\r\n    console.error(error);\r\n  });      // 删除请求 删除响应\r\n}\r\n\r\n// >>>>>>>>>>>>>> 删除功能结束\r\n\r\n\r\n\r\n// >>>>>>>>>>>>>> 修改功能开始\r\n\r\nconst newdata = ref({\r\n    id: '',\r\n    name: '',\r\n    shortName: '',\r\n    company: '',\r\n    managementFee: '',\r\n    manager: '',\r\n    nav: '',\r\n    redemptionFee: '',\r\n    subscriptionFee: '',\r\n    riskLevel: '',\r\n    salesFee: '',\r\n    startDate: '',\r\n    status: '',\r\n    totalShare: '',\r\n    type: '',\r\n    custodianFee: ''\r\n\r\n})\r\n\r\n\r\nconst open = ref(false);\r\n\r\nconst showModal = (record) => {\r\n  console.log('record',record)\r\n  open.value = true;\r\n  newdata.value = record;\r\n  if (newdata.value.riskLevel == '谨慎')\r\n    newdata.value.riskLevel = 0;\r\n    else if (newdata.value.riskLevel == '稳健')\r\n    newdata.value.riskLevel = 1;\r\n    else if (newdata.value.riskLevel == '平衡')\r\n    newdata.value.riskLevel = 2;\r\n    else if (newdata.value.riskLevel == '进取')\r\n    newdata.value.riskLevel = 3;\r\n    else if (newdata.value.riskLevel == '激进')\r\n    newdata.value.riskLevel = 4;\r\n\r\n  if (newdata.value.type == '股票型')\r\n    newdata.value.type = 0;\r\n   else if (newdata.value.type == '债券型')\r\n    newdata.value.type = 1;\r\n  else  if (newdata.value.type == '混合型')\r\n    newdata.value.type = 2;\r\n  else  if (newdata.value.type == '货币型')\r\n    newdata.value.type = 3;\r\n  else  if (newdata.value.type == 'ETF联接')\r\n    newdata.value.type = 4;\r\n  else  if (newdata.value.type == '其它')\r\n    newdata.value.type = 5;\r\n\r\n  if (newdata.value.status == '开放申购')\r\n    newdata.value.status = 0;\r\n  else if (newdata.value.status == '不可申购')\r\n    newdata.value.status = 1;\r\n\r\n};\r\n\r\nasync function handleOk() {\r\n  await modifyproduct();\r\n  console.log('newdata', newdata.value)\r\n  open.value = false;\r\n\r\n};\r\nfunction modifyproduct() {\r\n    newdata.value.custodianFee = parseFloat(newdata.value.custodianFee)\r\n    newdata.value.nav = parseFloat(newdata.value.nav)\r\n    newdata.value.redemptionFee = parseFloat(newdata.value.redemptionFee)\r\n    newdata.value.salesFee = parseFloat(newdata.value.salesFee)\r\n    newdata.value.subscriptionFee = parseFloat(newdata.value.subscriptionFee)\r\n    newdata.value.totalShare = parseFloat(newdata.value.totalShare)\r\n  newdata.value.managementFee = parseFloat(newdata.value.managementFee)\r\n    console.log(newdata.value)\r\n\r\n\r\n    axios.post('/product',newdata.value)\r\n        .then(response => {\r\n          const resp = response.data\r\n          console.log(resp)\r\n          if (resp.success) {\r\n            searchbyid();\r\n            message.success('修改产品成功', 5);\r\n            }\r\n            else {\r\n                message.info(`修改产品失败,${resp.message}`);\r\n        }\r\n\r\n        })\r\n        .catch(error => {\r\n          console.error(error);\r\n        });\r\n\r\n\r\n}\r\n\r\n\r\n// >>>>>>>>>>>>>> 修改功能结束\r\n\r\n\r\n// >>>>>>>>>>>>>添加净值功能开始\r\n\r\nimport * as echarts from 'echarts'\r\n\r\nconst addNav = ref({ id:'',date: \"\",nav: null});\r\nconst open2 = ref(false);\r\n\r\n//控制展开窗口\r\nconst showModa2 = (id) => {\r\n  open2.value = true;\r\n  addNav.value.id = id;\r\n\r\n}\r\n//控制关闭窗口\r\nasync function handleOk2() {\r\n  await navAdd()\r\n  open2.value = false;\r\n};\r\n\r\n\r\nfunction navAdd() {\r\n  addNav.value.nav = parseFloat(addNav.value.nav)\r\n  axios.put('/product/nav', addNav.value)\r\n    .then(response => {\r\n      const resp = response.data\r\n      console.log(resp)\r\n      if (resp.success) {\r\n        searchbyid();\r\n        message.success('添加成功', 5);\r\n      }\r\n      else {\r\n        message.info(`添加失败,${resp.message}`);\r\n      }\r\n\r\n    })\r\n    .catch(error => {\r\n      console.error(error);\r\n    });\r\n\r\n}\r\n\r\n\r\n\r\n\r\n// >>>>>>>>>添加产品净值结束\r\n\r\n\r\n\r\n\r\n\r\nconst columns = ref([\r\n  {\r\n    title:'产品代码',\r\n    dataIndex: 'id',\r\n    fixed: 'left',\r\n    // width: 100,\r\n    key : 'id'\r\n  },\r\n  {\r\n    title:'产品名称',\r\n    dataIndex: 'name',\r\n    fixed: 'left',\r\n    // width: 100,\r\n    key : 'name'\r\n  },\r\n  {\r\n    title:'产品简称',\r\n    dataIndex: 'shortName',\r\n    key : '1'\r\n  },\r\n  {\r\n    title:'发行公司',\r\n    dataIndex: 'company',\r\n    key : '2'\r\n\r\n  },\r\n  {\r\n    title:'管理费率',\r\n    dataIndex: 'managementFee',\r\n    key : '3'\r\n  },\r\n  {\r\n    title:'基金经理',\r\n    dataIndex: 'manager',\r\n    key : '4'\r\n\r\n  },\r\n  {\r\n    title:'产品净值',\r\n    dataIndex: 'nav',\r\n    key : '5'\r\n\r\n  },\r\n  {\r\n    title:'赎回费率',\r\n    dataIndex: 'redemptionFee',\r\n    key : '6'\r\n\r\n  },\r\n  {\r\n    title:'申购费率',\r\n    dataIndex: 'subscriptionFee',\r\n    key : '7'\r\n\r\n  },\r\n\r\n  {\r\n    title:'风险等级',\r\n    dataIndex: 'riskLevel',\r\n    key : '8'\r\n\r\n  },\r\n  {\r\n    title:'销售管理费率',\r\n    dataIndex: 'salesFee',\r\n    key : '9'\r\