{"ast":null,"code":"import axios from \"@/api/request\";\nimport { message } from \"ant-design-vue\";\nimport { ref, onMounted } from \"vue\";\nexport default {\n  __name: 'dialogTool',\n  props: [\"id\"],\n  emits: [\"ifShow\"],\n  setup(__props, {\n    expose: __expose,\n    emit: __emit\n  }) {\n    __expose();\n    const props = __props;\n    const emits = __emit;\n    const bo = ref(true);\n    const downloadurl = ref(\"\");\n    const downloadfilename = ref(\"\");\n    const title = ref({\n      id: \"\",\n      titleName: \"\",\n      projectName: \"\",\n      description: \"\",\n      textData: \"\",\n      imgPath: \"\",\n      svgPath: \"\",\n      voicePath: \"\"\n    });\n    const handelConfirm = () => {\n      emits(\"ifShow\", false);\n    };\n    const handleExport = () => {\n      // 发送请求到后端导出接口\n      axios.post(`/title/exportWord/${props.id}`).then(response => {\n        console.log(response.data);\n        downloadfilename.value = `${title.value.id}.docx`;\n        downloadurl.value = `http://localhost:7078/data/${title.value.id}/${title.value.id}.docx`;\n        var link = document.getElementById(\"download\");\n        console.log(link);\n        if (link) {\n          link.click();\n        }\n        message.success(\"导出成功\", 2);\n      }).catch(error => {\n        console.error(\"Error exporting document:\", error);\n        // 处理错误\n      });\n      emits(\"ifShow\", false);\n    };\n    onMounted(() => {\n      axios.get(`/title/getById/${props.id}`).then(response => {\n        console.log(response.data);\n        const data = response.data.data;\n        if (response.data.code == 200) {\n          title.value.id = data.id;\n          title.value.projectName = data.projectName;\n          title.value.titleName = data.titleName;\n          title.value.description = data.description;\n          title.value.textData = data.textData;\n          title.value.imgPath = data.imgPath;\n          title.value.svgPath = data.svgPath;\n          title.value.voicePath = data.voicePath;\n        } else {\n          emits(\"ifShow\", false);\n        }\n      }).catch(error => {\n        console.error(\"Error fetching titles:\", error);\n      });\n    });\n    const __returned__ = {\n      props,\n      emits,\n      bo,\n      downloadurl,\n      downloadfilename,\n      title,\n      handelConfirm,\n      handleExport,\n      get axios() {\n        return axios;\n      },\n      get message() {\n        return message;\n      },\n      ref,\n      onMounted\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["axios","message","ref","onMounted","props","__props","emits","__emit","bo","downloadurl","downloadfilename","title","id","titleName","projectName","description","textData","imgPath","svgPath","voicePath","handelConfirm","handleExport","post","then","response","console","log","data","value","link","document","getElementById","click","success","catch","error","get","code"],"sources":["H:/GitHub-Repository/hengsheng/hello-world/src/components/Tools/dialogTool.vue"],"sourcesContent":["<template>\r\n  <el-dialog\r\n    :model-value=\"bo\"\r\n    draggable\r\n    :close-on-click-modal=\"false\"\r\n    :show-close=\"false\"\r\n  >\r\n    <div class=\"father\">\r\n      <div class=\"header\">\r\n        <div class=\"firstrow\">\r\n          <span style=\"font-size: 14px\">题目编号：{{ title.id }}</span>\r\n          <span style=\"font-size: 14px\">课程名称：{{ title.projectName }}</span>\r\n        </div>\r\n      </div>\r\n      <div class=\"secondrow\">\r\n        <span style=\"font-size: 14px\">题目名称：{{ title.titleName }}</span>\r\n      </div>\r\n      <div class=\"description\" v-if=\"title.description\">\r\n        <span>题目描述：{{ title.description }}</span>\r\n      </div>\r\n      <!-- 文件展示部分 -->\r\n      <div class=\"file\">\r\n        <div v-if=\"title.textData\">\r\n          <span>题干：{{ title.textData }}</span>\r\n        </div>\r\n\r\n        <div v-if=\"title.imgPath\">\r\n          <p class=\"tip\">图像展示：</p>\r\n          <img\r\n            :src=\"'http://localhost:7078' + title.imgPath\"\r\n            alt=\"Image\"\r\n            class=\"img-container\"\r\n          />\r\n        </div>\r\n        <div v-if=\"title.voicePath\">\r\n          <p class=\"tip\">音频展示：</p>\r\n          <audio controls>\r\n            <source\r\n              :src=\"'http://localhost:7078' + title.voicePath\"\r\n              type=\"audio/mp3\"\r\n            />\r\n          </audio>\r\n        </div>\r\n        <div v-if=\"title.svgPath\">\r\n          <span class=\"tip\">图形展示：</span>\r\n          <!-- 使用 <object> 标签 -->\r\n          <object\r\n            :data=\"'http://localhost:7078' + title.svgPath\"\r\n            class=\"img-container\"\r\n            type=\"image/svg+xml\"\r\n          ></object>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <template #footer>\r\n      <span class=\"dialog-footer\">\r\n        <el-button @click=\"handleExport\">导出</el-button>\r\n        <a id=\"download\" :href=\"downloadurl\" :download=\"downloadfilename\"></a>\r\n        <el-button type=\"primary\" @click=\"handelConfirm\"> 确认 </el-button>\r\n      </span>\r\n    </template>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script setup>\r\nimport axios from \"@/api/request\";\r\nimport { message } from \"ant-design-vue\";\r\nimport { ref, defineProps, defineEmits, onMounted } from \"vue\";\r\n\r\nconst props = defineProps([\"id\"]);\r\nconst emits = defineEmits([\"ifShow\"]);\r\nconst bo = ref(true);\r\n\r\nconst downloadurl = ref(\"\");\r\nconst downloadfilename = ref(\"\");\r\n\r\nconst title = ref({\r\n  id: \"\",\r\n  titleName: \"\",\r\n  projectName: \"\",\r\n  description: \"\",\r\n\r\n  textData: \"\",\r\n  imgPath: \"\",\r\n  svgPath: \"\",\r\n  voicePath: \"\",\r\n});\r\n\r\nconst handelConfirm = () => {\r\n  emits(\"ifShow\", false);\r\n};\r\nconst handleExport = () => {\r\n  // 发送请求到后端导出接口\r\n  axios\r\n    .post(`/title/exportWord/${props.id}`)\r\n    .then((response) => {\r\n      console.log(response.data);\r\n      downloadfilename.value = `${title.value.id}.docx`;\r\n      downloadurl.value = `http://localhost:7078/data/${title.value.id}/${title.value.id}.docx`;\r\n      var link = document.getElementById(\"download\");\r\n      console.log(link);\r\n      if (link) {\r\n        link.click();\r\n      }\r\n      message.success(\"导出成功\", 2);\r\n    })\r\n    .catch((error) => {\r\n      console.error(\"Error exporting document:\", error);\r\n      // 处理错误\r\n    });\r\n  emits(\"ifShow\", false);\r\n};\r\nonMounted(() => {\r\n  axios\r\n    .get(`/title/getById/${props.id}`)\r\n    .then((response) => {\r\n      console.log(response.data);\r\n      const data = response.data.data;\r\n      if (response.data.code == 200) {\r\n        title.value.id = data.id;\r\n        title.value.projectName = data.projectName;\r\n        title.value.titleName = data.titleName;\r\n        title.value.description = data.description;\r\n        title.value.textData = data.textData;\r\n        title.value.imgPath = data.imgPath;\r\n        title.value.svgPath = data.svgPath;\r\n        title.value.voicePath = data.voicePath;\r\n      } else {\r\n        emits(\"ifShow\", false);\r\n      }\r\n    })\r\n    .catch((error) => {\r\n      console.error(\"Error fetching titles:\", error);\r\n    });\r\n});\r\n</script>\r\n\r\n<style scoped>\r\n.father {\r\n  display: flex;\r\n  padding: 10px; /* 上下左右内边距为 10px */\r\n  flex-direction: column;\r\n\r\n  gap: 15px; /* 元素之间的间距为 10px */\r\n}\r\n\r\n.file {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  flex-direction: column;\r\n  gap: 10px; /* 元素之间的间距为 10px */\r\n  justify-content: flex-start;\r\n}\r\n\r\n.img-container {\r\n  max-width: 60%; /* 图像最大宽度为父容器宽度 */\r\n  max-height: 25%; /* 图像最大高度为父容器高度 */\r\n  display: block; /* 设置为块级元素，使max-width和max-height生效 */\r\n  margin: auto; /* 居中显示 *