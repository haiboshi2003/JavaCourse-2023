hello-world/src/components/Tools/dialogTool.vue"],"sourcesContent":["<template>\r\n  <el-dialog v-model=\"ifshow\" title=\"Shipping address\">\r\n    <div class=\"card-header\">\r\n      <div class=\"firstrow\">\r\n        <span style=\"font-size: 14px\">课程名称：{{ title.projectName }}</span>\r\n\r\n        <span style=\"font-size: 14px\">题目编号：{{ title.id }}</span>\r\n      </div>\r\n      <div class=\"secondrow\">\r\n        <span style=\"font-size: 13px\">题目名称：{{ title.titleName }}</span>\r\n      </div>\r\n    </div>\r\n    <div class=\"description\">\r\n      <span>{{ title.description }}</span>\r\n    </div>\r\n\r\n    <!-- 文件展示部分 -->\r\n    <div class=\"file\">\r\n      <div v-if=\"title.imgPath\">\r\n        <p>图像展示：</p>\r\n        <img :src=\"'http://localhost:7078' + title.imgPath\" alt=\"Image\" />\r\n      </div>\r\n      <div v-if=\"title.voicePath\">\r\n        <p>音频展示：</p>\r\n        <audio controls>\r\n          <source\r\n            :src=\"'http://localhost:7078' + title.voicePath\"\r\n            type=\"audio/mp3\"\r\n          />\r\n        </audio>\r\n      </div>\r\n      <div v-if=\"title.svgPath\">\r\n        <p>图形展示：</p>\r\n        <!-- 使用 <object> 标签 -->\r\n        <object\r\n          :data=\"'http://localhost:7078' + title.svgPath\"\r\n          type=\"image/svg+xml\"\r\n          width=\"100%\"\r\n          height=\"100%\"\r\n        ></object>\r\n      </div>\r\n    </div>\r\n    <template #footer>\r\n      <span class=\"dialog-footer\">\r\n        <el-button @click=\"ifshow = false\">Cancel</el-button>\r\n        <el-button type=\"primary\" @click=\"ifshow = false\"> Confirm </el-button>\r\n      </span>\r\n    </template>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script setup>\r\nimport axios from \"@/api/request\";\r\nimport { message } from \"ant-design-vue\";\r\nimport { ref, defineProps, defineEmits, onMounted } from \"vue\";\r\n\r\nconst ifshow = ref(true);\r\nconst props = defineProps([\"id\"]);\r\nconst title = ref({\r\n  id: \"\",\r\n  titleName: \"\",\r\n  projectName: \"\",\r\n  description: \"\",\r\n\r\n  textdata: \"\",\r\n  imgPath: \"\",\r\n  svgPath: \"\",\r\n  voicePath: \"\",\r\n});\r\n\r\nonMounted(() => {\r\n  axios\r\n    .get(`/title/getById/${props.id}`)\r\n    .then((response) => {\r\n      console.log(response.data);\r\n      const data = response.data.data;\r\n      if (response.data.code == 200) {\r\n        title.value.id = data.id;\r\n        title.value.projectName = data.projectName;\r\n        title.value.titleName = data.titleName;\r\n        title.value.description = data.description;\r\n        title.value.textdata = data.textdata;\r\n        title.value.imgPath = data.imgPath;\r\n        title.value.svgPath = data.svgPath;\r\n        title.value.voicePath = data.voicePath;\r\n      } else {\r\n        ifshow.value = false;\r\n      }\r\n    })\r\n    .catch((error) => {\r\n      console.error(\"Error fetching titles:\", error);\r\n    });\r\n});\r\n</script>\r\n\r\n<style scoped>\r\n.card-header {\r\n  display: flex;\r\n  justify-content: center;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  height: 50px;\r\n  gap: 20px; /* 元素之间的间距为 10px */\r\n}\r\n.footer {\r\n  display: flex;\r\n  justify-content: center;\r\n  flex-direction: row;\r\n  align-items: center;\r\n  gap: 10px; /* 元素之间的间距为 10px */\r\n}\r\n\r\n.firstrow {\r\n  display: flex;\r\n  flex-direction: row;\r\n  align-items: center;\r\n  gap: 50px; /* 元素之间的间距为 10px */\r\n}\r\n.description {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n</style>\r\n"],"mappings":"AAoDA,OAAOA,KAAK,MAAM,eAAe;AACjC,SAASC,OAAO,QAAQ,gBAAgB;AACxC,SAASC,GAA6B,EAAEC,SAAS,QAAQ,KAAK;;;;;;;;IAE9D,MAAMC,MAAM,GAAGF,GAAG,CAAC,IAAI,CAAC;IACxB,MAAMG,KAAK,GAAGC,OAAmB;IACjC,MAAMC,KAAK,GAAGL,GAAG,CAAC;MAChBM,EAAE,EAAE,EAAE;MACNC,SAAS,EAAE,EAAE;MACbC,WAAW,EAAE,EAAE;MACfC,WAAW,EAAE,EAAE;MAEfC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE,EAAE;MACXC,OAAO,EAAE,EAAE;MACXC,SAAS,EAAE;IACb,CAAC,CAAC;IAEFZ,SAAS,CAAC,MAAM