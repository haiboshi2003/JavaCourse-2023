{"ast":null,"code":"import { ref, computed, onMounted } from 'vue';\nimport axios from \"../api/request\";\nimport { message } from 'ant-design-vue';\nexport default {\n  __name: 'searchcustomer',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const dto = ref({\n      current: 1,\n      size: 5\n    });\n    const data = ref([]);\n    const data2 = ref([]);\n    const searchid = ref('');\n\n    //>>>>>>>>type等含义转化\n    function change(data) {\n      for (let i = 0; i < data.value.length; i++) {\n        if (data.value[i].riskLevel === 0) data.value[i].riskLevel = '谨慎';else if (data.value[i].riskLevel === 1) data.value[i].riskLevel = '稳健';else if (data.value[i].riskLevel === 2) data.value[i].riskLevel = '平衡';else if (data.value[i].riskLevel === 3) data.value[i].riskLevel = '进取';else if (data.value[i].riskLevel === 4) data.value[i].riskLevel = '激进';\n        if (data.value[i].status === 0) data.value[i].status = '正常';else if (data.value[i].status === 1) data.value[i].status = '冻结';\n        if (data.value[i].type === 0) data.value[i].type = '个人客户';else if (data.value[i].type === 1) data.value[i].type = '公司客户';\n        if (data.value[i].address == '') data.value[i].address = '空';\n        if (data.value[i].email == '') data.value[i].email = '空';\n        if (data.value[i].name == '') data.value[i].name = '空';\n        if (data.value[i].phone == '') data.value[i].phone = '空';\n        if (data.value[i].registration == '') data.value[i].registration = '空';\n      }\n      return data;\n    }\n\n    // >>>>>>>>>数据获取\n    async function getdata() {\n      try {\n        const resp = await axios.get('/customer', {\n          params: {\n            current: dto.value.current,\n            size: dto.value.size\n          }\n        });\n        //提出响应数据对象\n        data2.value = resp.data;\n        data.value = resp.data;\n        // 处理数据\n        console.log('获取成功');\n        console.log(data.value);\n        change(data);\n      } catch (error) {\n        // 处理错误\n        console.error(error);\n      }\n    }\n\n    // >>>>>>>>组件加载后第一次获取数据\n    onMounted(() => {\n      getdata();\n    });\n    const customers = computed(() => {\n      return data.value || [];\n    });\n\n    // >>>>>>>>用于分页\n    const pagination = computed(() => {\n      return {\n        current: dto.value.current,\n        // 当前页\n        pageSize: dto.value.size,\n        // 页大小\n        total: customers.length,\n        // 总记录数\n        showSizeChanger: true,\n        // 显示页大小的下拉列表\n        pageSizeOptions: [\"3\", \"5\", \"10\", \"50\", \"100\"] // 自定义下拉列表内容\n      };\n    });\n    // 在页号或页大小改变时调用\n    function tableChange(pagination) {\n      console.log(pagination);\n      dto.value.current = pagination.current ?? 1;\n      dto.value.size = pagination.pageSize ?? 5;\n      // run(dto.value)\n    }\n\n    // >>>>>>>>>>>>>>>>>>>>>>>搜索功能开始\n    async function searchbyname() {\n      console.log(searchid);\n      console.log(typeof searchid);\n      await getdata();\n      if (searchid == '') {\n        data.value = data2.value;\n        change(data);\n      } else {\n        data.value = data2.value.filter(number => {\n          return number.name.startsWith(searchid.value);\n        });\n        change(data);\n      }\n    }\n\n    // >>>>>>>>>>>>>>>>>>>>>>>>>搜索功结束\n\n    // >>>>>>>>>>>>>> 销户功能开始\n    function onDelete(myid) {\n      console.log(myid);\n      axios.delete('/customer', {\n        data: myid\n      }).then(response => {\n        console.log(response.data);\n        searchbyname(); //                   查询请求 查询响应\n        if (response.data.success) message.success('删除成功');else message.error('删除失败');\n      }).catch(error => {\n        console.error(error);\n      }); // 删除请求 删除响应\n    }\n\n    // >>>>>>>>>>>>>> 删销户功能结束\n\n    // >>>>>>>>>>>>>> 修改功能开始\n\n    const newdata = ref({\n      address: \"\",\n      email: \"\",\n      id: \"\",\n      name: \"\",\n      phone: \"\",\n      registration: \"\",\n      riskLevel: 0,\n      status: 0,\n      type: 0\n    });\n    const open = ref(false);\n\n    //打开对话框\n    const showModal = record => {\n      open.value = true;\n      console.log(record);\n      newdata.value = record;\n    };\n\n    //确认事件\n    async function handleOk() {\n      if (newdata.value.riskLevel == '谨慎') newdata.value.riskLevel = 0;else if (newdata.value.riskLevel == '稳健') newdata.value.riskLevel = 1;else if (newdata.value.riskLevel == '平衡') newdata.value.riskLevel = 2;else if (newdata.value.riskLevel == '进取') newdata.value.riskLevel = 3;else if (newdata.value.riskLevel == '激进') newdata.value.riskLevel = 4;\n      if (newdata.value.type == '个人客户') newdata.value.type = 0;else if (newdata.value.type == '公司客户') newdata.value.type = 1;\n      if (newdata.value.status == '正常') newdata.value.status = 0;else if (newdata.value.status == '冻结') newdata.value.status = 1;\n      open.value = false;\n      await modifycostomer();\n      console.log('newdata', newdata.value);\n    }\n    ;\n\n    //发送修改信息请求\n    function modifycostomer() {\n      axios.post('/customer', newdata.value).then(response => {\n        const resp = response.data;\n        console.log(resp);\n        if (resp.success) {\n          searchbyname();\n          message.success('修改客户信息成功', 5);\n        } else {\n          message.info(`修改客户信息失败,${resp.message}`);\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n\n    // >>>>>>>>>>>>>> 修改功能结束\n\n    // >>>>>>>>>>>>表格列\n    const columns = ref([{\n      title: '名称',\n      dataIndex: 'name',\n      key: 'name'\n    }, {\n      title: '证件号',\n      dataIndex: 'id',\n      key: 'id'\n    }, {\n      title: '电子邮箱',\n      dataIndex: 'email',\n      key: '1'\n    }, {\n      title: '通讯地址',\n      dataIndex: 'address',\n      key: '0'\n    }, {\n      title: '联系电话',\n      dataIndex: 'phone',\n      key: '2'\n    }, {\n      title: '注册日期',\n      dataIndex: 'registration',\n      key: '3'\n    }, {\n      title: '风险等级',\n      dataIndex: 'riskLevel',\n      key: '4'\n    }, {\n      title: '客户状态',\n      dataIndex: 'status',\n      key: '5'\n    }, {\n      title: '客户类型',\n      dataIndex: 'type',\n      key: '6'\n    }, {\n      title: '操作',\n      dataIndex: 'operation',\n      key: '14',\n      fixed: 'right'\n      // width: 100,\n    }]);\n    const __returned__ = {\n      dto,\n      data,\n      data2,\n      searchid,\n      change,\n      getdata,\n      customers,\n      pagination,\n      tableChange,\n      searchbyname,\n      onDelete,\n      newdata,\n      open,\n      showModal,\n      handleOk,\n      modifycostomer,\n      columns,\n      ref,\n      computed,\n      onMounted,\n      get axios() {\n        return axios;\n      },\n      get message() {\n        return message;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","axios","message","dto","current","size","data","data2","searchid","change","i","value","length","riskLevel","status","type","address","email","name","phone","registration","getdata","resp","get","params","console","log","error","customers","pagination","pageSize","total","showSizeChanger","pageSizeOptions","tableChange","searchbyname","filter","number","startsWith","onDelete","myid","delete","then","response","success","catch","newdata","id","open","showModal","record","handleOk","modifycostomer","post","info","columns","title","dataIndex","key","fixed"],"sources":["H:/GitHub-Repository/hengsheng/hello-world/src/components/searchcustomer.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <br />\r\n\r\n    <!-- 搜索引擎 -->\r\n    <a-row>\r\n      <a-col :span=\"16\"></a-col>\r\n      <a-col :span=\"5\">\r\n        <a-input\r\n          v-model:value=\"searchid\"\r\n          placeholder=\"输入客户名称\"\r\n          size=\"large\"\r\n          style=\"width: 340px\"\r\n        ></a-input>\r\n      </a-col>\r\n      <a-col :span=\"1\"></a-col>\r\n\r\n      <a-col :span=\"2\">\r\n        <a-button @click=\"searchbyname\" type=\"primary\" size=\"large\"\r\n          >搜索</a-button\r\n        >\r\n      </a-col>\r\n    </a-row>\r\n    <hr />\r\n\r\n    <!-- 表格 -->\r\n    <div class=\"tb-container\">\r\n      <a-table\r\n        :columns=\"columns\"\r\n        :dataSource=\"customers\"\r\n        rowKey=\"id\"\r\n        :pagination=\"pagination\"\r\n        :scroll=\"{ y: 450 }\"\r\n        @change=\"tableChange\"\r\n      >\r\n        <template #bodyCell=\"{ column, record }\">\r\n          <template v-if=\"column.dataIndex === 'operation'\">\r\n            <a-popconfirm\r\n              placement=\"left\"\r\n              title=\"确认要修改该用户信息吗?\"\r\n              ok-text=\"确定\"\r\n              cancel-text=\"取消\"\r\n              @confirm=\"showModal(record)\"\r\n            >\r\n              <a>修改</a>\r\n            </a-popconfirm>\r\n            <a-divider type=\"vertical\"></a-divider>\r\n\r\n            <a-popconfirm\r\n              placement=\"left\"\r\n              title=\"确认要销户吗?\"\r\n              ok-text=\"确定\"\r\n              cancel-text=\"取消\"\r\n              @confirm=\"onDelete(record.id)\"\r\n            >\r\n              <a>销户</a>\r\n            </a-popconfirm>\r\n          </template>\r\n        </template>\r\n      </a-table>\r\n    </div>\r\n\r\n    <!-- 修改功能 -->\r\n    <div>\r\n      <a-modal\r\n        v-model:open=\"open\"\r\n        title=\"修改产品信息\"\r\n        ok-text=\"确定修改\"\r\n        cancel-text=\"取消\"\r\n        width=\"1200px\"\r\n        @ok=\"handleOk\"\r\n      >\r\n        <div>\r\n          <br />\r\n          <a-row>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <label for=\"150\">客户证件号</label>\r\n              <a-input v-model:value=\"newdata.id\" id=\"150\" :disabled=\"true\" />\r\n            </a-col>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <label for=\"151\">地址</label>\r\n              <a-input v-model:value=\"newdata.address\" id=\"151\" />\r\n            </a-col>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <label for=\"152\">邮箱</label>\r\n              <a-input v-model:value=\"newdata.email\" id=\"152\" />\r\n            </a-col>\r\n          </a-row>\r\n\r\n          <br />\r\n          <br />\r\n\r\n          <a-row>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <label for=\"153\">名称</label>\r\n              <a-input v-model:value=\"newdata.name\" id=\"153\" />\r\n            </a-col>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <label for=\"154\">联系电话</label>\r\n              <a-input v-model:value=\"newdata.phone\" id=\"154\" />\r\n            </a-col>\r\n          </a-row>\r\n\r\n          <br />\r\n          <br />\r\n\r\n          <a-row>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"7\">\r\n              <label for=\"riskLevel\">风险等级：</label>\r\n              <a-select\r\n                id=\"riskLevel\"\r\n                ref=\"select\"\r\n                v-model:value=\"newdata.riskLevel\"\r\n                style=\"width: 250px\"\r\n              >\r\n                <a-select-option :value=\"Number(0)\">谨慎</a-select-option>\r\n                <a-select-option :value=\"Number(1)\">稳健</a-select-option>\r\n                <a-select-option :value=\"Number(2)\">平稳</a-select-option>\r\n                <a-select-option :value=\"Number(3)\">进取</a-select-option>\r\n                <a-select-option :value=\"Number(4)\">激进</a-select-option>\r\n              </a-select>\r\n            </a-col>\r\n          </a-row>\r\n\r\n          <br />\r\n          <br />\r\n\r\n          <a-row>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <label for=\"status\">客户状态：</label>\r\n              <a-select\r\n                id=\"status\"\r\n                ref=\"select\"\r\n                v-model:value=\"newdata.status\"\r\n                style=\"width: 250px\"\r\n              >\r\n                <a-select-option :value=\"Number(0)\">正常</a-select-option>\r\n                <a-select-option :value=\"Number(1)\">冻结</a-select-option>\r\n              </a-select>\r\n            </a-col>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <label for=\"type\">客户类型：</label>\r\n              <a-select\r\n                ref=\"select\"\r\n                id=\"type\"\r\n                v-model:value=\"newdata.type\"\r\n                style=\"width: 250px\"\r\n              >\r\n                <a-select-option :value=\"Number('0')\">个人客户</a-select-option>\r\n                <a-select-option :value=\"Number('1')\">公司客户</a-select-option>\r\n              </a-select>\r\n            </a-col>\r\n          </a-row>\r\n          <br />\r\n          <br />\r\n          <a-row>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <label for=\"155\">请选择注册日期</label>\r\n              <a-date-picker\r\n                :value-format=\"'YYYY-MM-DD'\"\r\n                id=\"155\"\r\n                v-model:value=\"newdata.registration\"\r\n                style=\"width: 250px\"\r\n              />\r\n            </a-col>\r\n          </a-row>\r\n          <br />\r\n        </div>\r\n      </a-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup lang=\"js\">\r\n\r\nimport { ref, computed, onMounted} from 'vue';\r\nimport axios from \"../api/request\";\r\nimport { message } from 'ant-design-vue';\r\n\r\n\r\nconst dto = ref({ current: 1, size: 5})\r\nconst data = ref([]);\r\nconst data2 = ref([]);\r\nconst searchid = ref('')\r\n\r\n\r\n//>>>>>>>>type等含义转化\r\nfunction change(data) {\r\n\r\n  for (let i = 0; i < data.value.length; i++) {\r\n    if (data.value[i].riskLevel === 0)\r\n      data.value[i].riskLevel = '谨慎'\r\n    else if (data.value[i].riskLevel === 1)\r\n      data.value[i].riskLevel = '稳健'\r\n    else if (data.value[i].riskLevel === 2)\r\n      data.value[i].riskLevel = '平衡'\r\n    else if (data.value[i].riskLevel === 3)\r\n      data.value[i].riskLevel = '进取'\r\n    else if (data.value[i].riskLevel === 4)\r\n      data.value[i].riskLevel = '激进'\r\n\r\n    if (data.value[i].status === 0)\r\n      data.value[i].status = '正常'\r\n    else if (data.value[i].status === 1)\r\n      data.value[i].status = '冻结'\r\n\r\n    if (data.value[i].type === 0)\r\n      data.value[i].type = '个人客户'\r\n    else if (data.value[i].type === 1)\r\n      data.value[i].type = '公司客户'\r\n\r\n    if (data.value[i].address == '')\r\n      data.value[i].address = '空';\r\n\r\n      if (data.value[i].email == '')\r\n      data.value[i].email = '空';\r\n\r\n    if (data.value[i].name == '')\r\n      data.value[i].name = '空';\r\n\r\n    if (data.value[i].phone == '')\r\n      data.value[i].phone = '空';\r\n\r\n    if (data.value[i].registration == '')\r\n      data.value[i].registration = '空';\r\n\r\n  }\r\n  return data\r\n\r\n}\r\n\r\n// >>>>>>>>>数据获取\r\nasync function getdata() {\r\n  try {\r\n\r\n    const resp = await axios.get('/customer', {\r\n      params: {\r\n        current: dto.value.current,\r\n        size: dto.value.size\r\n      }\r\n    })\r\n    //提出响应数据对象\r\n    data2.value = resp.data;\r\n    data.value = resp.data;\r\n    // 处理数据\r\n    console.log('获取成功');\r\n    console.log(data.value);\r\n    change(data);\r\n\r\n\r\n  } catch (error) {\r\n    // 处理错误\r\n    console.error(error);\r\n\r\n\r\n  }\r\n}\r\n\r\n// >>>>>>>>组件加载后第一次获取数据\r\nonMounted(() => {\r\n    getdata()\r\n})\r\n\r\nconst customers = computed(()=>{\r\n  return data.value|| []\r\n})\r\n\r\n// >>>>>>>>用于分页\r\nconst pagination = computed(()=>{\r\n  return {\r\n    current: dto.value.current, // 当前页\r\n    pageSize: dto.value.size, // 页大小\r\n    total: customers.length,       // 总记录数\r\n    showSizeChanger: true,    // 显示页大小的下拉列表\r\n    pageSizeOptions: [\"3\",\"5\",\"10\",\"50\",\"100\"] // 自定义下拉列表内容\r\n  }\r\n})\r\n// 在页号或页大小改变时调用\r\nfunction tableChange(pagination) {\r\n  console.log(pagination)\r\n  dto.value.current = pagination.current ?? 1\r\n  dto.value.size = pagination.pageSize ?? 5\r\n  // run(dto.value)\r\n}\r\n\r\n// >>>>>>>>>>>>>>>>>>>>>>>搜索功能开始\r\nasync function searchbyname() {\r\n  console.log(searchid)\r\n  console.log(typeof (searchid));\r\n  await getdata()\r\n  if (searchid == '') {\r\n    data.value = data2.value;\r\n    change(data);\r\n\r\n  }\r\n  else {\r\n    data.value = data2.value.filter((number) => {\r\n      return number.name.startsWith(searchid.value);\r\n    });\r\n    change(data);\r\n\r\n  }\r\n\r\n}\r\n\r\n// >>>>>>>>>>>>>>>>>>>>>>>>>搜索功结束\r\n\r\n\r\n// >>>>>>>>>>>>>> 销户功能开始\r\nfunction onDelete(myid) {\r\n  console.log(myid);\r\n\r\n  axios.delete('/customer', {data:myid})\r\n  .then(response => {\r\n    console.log(response.data);\r\n\r\n    searchbyname()       //                   查询请求 查询响应\r\n    if (response.data.success)\r\n      message.success('删除成功')\r\n    else\r\n      message.error('删除失败')\r\n\r\n\r\n  })\r\n  .catch(error => {\r\n    console.error(error);\r\n  });      // 删除请求 删除响应\r\n}\r\n\r\n// >>>>>>>>>>>>>> 删销户功能结束\r\n\r\n\r\n// >>>>>>>>>>>>>> 修改功能开始\r\n\r\nconst newdata = ref({\r\n    address: \"\",\r\n    email: \"\",\r\n    id: \"\",\r\n    name: \"\",\r\n    phone: \"\",\r\n    registration: \"\",\r\n    riskLevel: 0,\r\n    status: 0,\r\n    type: 0\r\n\r\n})\r\n\r\nconst open = ref(false);\r\n\r\n//打开对话框\r\nconst showModal = (record) => {\r\n  open.value = true;\r\n  console.log(record)\r\n  newdata.value = record;\r\n};\r\n\r\n//确认事件\r\nasync function handleOk() {\r\n  if (newdata.value.riskLevel == '谨慎')\r\n    newdata.value.riskLevel = 0;\r\n    else if (newdata.value.riskLevel == '稳健')\r\n    newdata.value.riskLevel = 1;\r\n    else if (newdata.value.riskLevel == '平衡')\r\n    newdata.value.riskLevel = 2;\r\n    else if (newdata.value.riskLevel == '进取')\r\n    newdata.value.riskLevel = 3;\r\n    else if (newdata.value.riskLevel == '激进')\r\n    newdata.value.riskLevel = 4;\r\n\r\n  if (newdata.value.type == '个人客户')\r\n    newdata.value.type = 0;\r\n   else if (newdata.value.type == '公司客户')\r\n    newdata.value.type = 1;\r\n\r\n\r\n  if (newdata.value.status == '正常')\r\n    newdata.value.status = 0;\r\n  else if (newdata.value.status == '冻结')\r\n    newdata.value.status = 1;\r\n  open.value = false;\r\n  await modifycostomer();\r\n  console.log('newdata',newdata.value)\r\n};\r\n\r\n//发送修改信息请求\r\nfunction modifycostomer() {\r\n\r\n\r\n    axios.post('/customer',newdata.value)\r\n        .then(response => {\r\n          const resp = response.data\r\n          console.log(resp)\r\n          if (resp.success) {\r\n            searchbyname();\r\n            message.success('修改客户信息成功', 5);\r\n            }\r\n            else {\r\n                message.info(`修改客户信息失败,${resp.message}`);\r\n        }\r\n\r\n        })\r\n        .catch(error => {\r\n          console.error(error);\r\n        });\r\n\r\n\r\n}\r\n\r\n\r\n// >>>>>>>>>>>>>> 修改功能结束\r\n\r\n\r\n\r\n\r\n// >>>>>>>>>>>>表格列\r\nconst columns = ref([\r\n  {\r\n    title:'名称',\r\n    dataIndex: 'name',\r\n    key : 'name'\r\n    },\r\n\r\n  {\r\n    title:'证件号',\r\n    dataIndex: 'id',\r\n    key : 'id'\r\n    },\r\n\r\n  {\r\n    title:'电子邮箱',\r\n    dataIndex: 'email',\r\n    key : '1'\r\n    },\r\n    {\r\n    title:'通讯地址',\r\n    dataIndex: 'address',\r\n    key : '0'\r\n  },\r\n  {\r\n    title:'联系电话',\r\n    dataIndex: 'phone',\r\n    key : '2'\r\n\r\n  },\r\n  {\r\n    title:'注册日期',\r\n    dataIndex: 'registration',\r\n    key : '3'\r\n  },\r\n  {\r\n    title:'风险等级',\r\n    dataIndex: 'riskLevel',\r\n    key : '4'\r\n\r\n  },\r\n  {\r\n    title:'客户状态',\r\n    dataIndex: 'status',\r\n    key : '5'\r\n\r\n  },\r\n  {\r\n    title