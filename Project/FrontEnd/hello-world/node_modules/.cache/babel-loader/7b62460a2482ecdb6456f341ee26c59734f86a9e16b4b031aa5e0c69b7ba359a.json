{"ast":null,"code":"import { ref, computed, onMounted } from 'vue';\nimport axios from \"../api/request\";\nimport { message } from 'ant-design-vue';\nimport SelectUserModal from \"@/components/Business/Tools/SelectUserModal.vue\";\nexport default {\n  __name: 'Bank-Account',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const sumShow = ref(false);\n    const selectedCustomer = ref({\n      id: \"\",\n      name: \"\"\n    });\n    const dto = ref({\n      current: 1,\n      size: 5,\n      id: ''\n    });\n    const data = ref([]);\n\n    // >>>>>>>>>数据获取\n    async function getdata() {\n      try {\n        const resp = await axios.get('/account/all', {\n          params: {\n            current: dto.value.current,\n            size: dto.value.size\n          }\n        });\n        //提出响应数据对象\n        data.value = resp.data;\n        // 处理数据\n        console.log('获取成功');\n        console.log(data.value);\n      } catch (error) {\n        // 处理错误\n        console.error(error);\n      }\n    }\n\n    // >>>>>>>>组件加载后第一次获取数据\n    onMounted(() => {\n      getdata();\n    });\n    const bankaccount = computed(() => {\n      return data.value || [];\n    });\n\n    // >>>>>>>>用于分页\n    const pagination = computed(() => {\n      return {\n        current: dto.value.current,\n        // 当前页\n        pageSize: dto.value.size,\n        // 页大小\n        total: bankaccount.length,\n        // 总记录数\n        showSizeChanger: true,\n        // 显示页大小的下拉列表\n        pageSizeOptions: [\"3\", \"5\", \"10\", \"50\", \"100\"] // 自定义下拉列表内容\n      };\n    });\n    // 在页号或页大小改变时调用\n    function tableChange(pagination) {\n      console.log(pagination);\n      dto.value.current = pagination.current ?? 1;\n      dto.value.size = pagination.pageSize ?? 5;\n      // run(dto.value)\n    }\n\n    // >>>>>>>>>>>>>>>>>>>>>>>搜索功能开始\n    async function searchbyname() {\n      console.log(dto.value.id);\n      console.log(typeof dto.value.id);\n      if (dto.value.id == '') {\n        await getdata();\n      } else {\n        axios.get('/account', {\n          params: {\n            id: dto.value.id,\n            current: dto.value.current,\n            size: dto.value.size\n          }\n        }).then(resp => {\n          //提出响应数据对象\n          data.value = resp.data;\n          // 处理数据\n          console.log('获取成功');\n          console.log(data.value);\n        }).catch(error => {\n          // 处理错误\n          console.error(error);\n        });\n      }\n    }\n\n    // >>>>>>>>>>>>>>>>>>>>>>>>>搜索功结束\n\n    // >>>>>>>>>>>>>> 销户功能开始\n    function onDelete(myaccount, myid) {\n      console.log(myaccount);\n      console.log(myid);\n      axios.delete('/account', {\n        data: {\n          id: myid,\n          account: myaccount\n        }\n      }).then(response => {\n        console.log(response.data);\n        if (response.data.success) {\n          message.success('删除成功');\n          searchbyname(); //                   查询请求 查询响应\n        } else message.error('删除失败');\n      }).catch(error => {\n        console.error(error);\n      }); // 删除请求 删除响应\n    }\n\n    // >>>>>>>>>>>>>> 删销户功能结束\n\n    // >>>>>>>>>>>>>> 修改功能开始\n\n    const newdata = ref({\n      id: \"\",\n      account: \"\",\n      balance: \"\",\n      bank: \"\",\n      branch: \"\"\n    });\n    const open = ref(false);\n\n    //打开对话框\n    const showModal = (account, id) => {\n      open.value = true;\n      newdata.value.account = account;\n      newdata.value.id = id;\n    };\n\n    //确认事件\n    async function handleOk() {\n      await modifycostomer();\n      searchbyname();\n      console.log('newdata', newdata.value);\n      open.value = false;\n    }\n    ;\n\n    //发送修改信息请求\n    function modifycostomer() {\n      newdata.value.balance = parseFloat(newdata.value.balance);\n      axios.post('/account', newdata.value).then(response => {\n        const resp = response.data;\n        console.log(resp);\n        if (resp.success) {\n          searchbyname();\n          message.success('修改账户信息成功', 5);\n        } else {\n          message.info(`修改账户信息失败,${resp.message}`);\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n\n    // >>>>>>>>>>>>>> 修改功能结束\n\n    // >>>>>>>>>>>>>新增功能开始\n\n    const open2 = ref(false);\n    const showModal2 = () => {\n      open2.value = true;\n    };\n    async function handleOk2() {\n      //   console.log(e);\n      await add();\n      open2.value = false;\n    }\n\n    //新增账户信息\n    const newaccount = ref({\n      id: \"\",\n      account: \"\",\n      balance: \"\",\n      bank: \"\",\n      branch: \"\"\n    });\n\n    //新增账户函数\n    function add() {\n      newaccount.value.balance = parseFloat(newaccount.value.balance);\n      console.log(newaccount.value);\n      axios.put('/account', newaccount.value).then(response => {\n        const resp = response.data;\n        console.log(resp);\n        if (resp.success) {\n          message.success('添加账户成功', 5);\n          searchbyname();\n        } else {\n          message.error(`添加账户失败,${resp.message}`);\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n\n    // >>>>>>>>>>>>新增功能结束\n\n    // >>>>>>>>>>>>表格列\n    const columns = ref([{\n      title: '卡号',\n      dataIndex: 'account',\n      key: 'account'\n    }, {\n      title: '客户证件号或代码',\n      dataIndex: 'id',\n      key: 'id'\n    }, {\n      title: '账户余额',\n      dataIndex: 'balance',\n      key: '1'\n    }, {\n      title: '银行名称',\n      dataIndex: 'bank',\n      key: '0'\n    }, {\n      title: '分行名称',\n      dataIndex: 'branch',\n      key: '2'\n    }, {\n      title: '操作',\n      dataIndex: 'operation',\n      key: '4',\n      fixed: 'right'\n      // width: 100,\n    }]);\n    const __returned__ = {\n      sumShow,\n      selectedCustomer,\n      dto,\n      data,\n      getdata,\n      bankaccount,\n      pagination,\n      tableChange,\n      searchbyname,\n      onDelete,\n      newdata,\n      open,\n      showModal,\n      handleOk,\n      modifycostomer,\n      open2,\n      showModal2,\n      handleOk2,\n      newaccount,\n      add,\n      columns,\n      ref,\n      computed,\n      onMounted,\n      get axios() {\n        return axios;\n      },\n      get message() {\n        return message;\n      },\n      SelectUserModal\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","axios","message","SelectUserModal","sumShow","selectedCustomer","id","name","dto","current","size","data","getdata","resp","get","params","value","console","log","error","bankaccount","pagination","pageSize","total","length","showSizeChanger","pageSizeOptions","tableChange","searchbyname","then","catch","onDelete","myaccount","myid","delete","account","response","success","newdata","balance","bank","branch","open","showModal","handleOk","modifycostomer","parseFloat","post","info","open2","showModal2","handleOk2","add","newaccount","put","columns","title","dataIndex","key","fixed"],"sources":["H:/GitHub-Repository/hengsheng/hello-world/src/components/Bank-Account.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <br />\r\n\r\n    <!-- 引擎 -->\r\n    <a-row>\r\n      <a-col :span=\"1\"></a-col>\r\n      <a-col :span=\"4\">\r\n        <a-button type=\"primary\" @click=\"showModal2\" size=\"large\"\r\n          >添加账户</a-button\r\n        >\r\n      </a-col>\r\n      <a-col :span=\"11\"></a-col>\r\n      <a-col :span=\"5\">\r\n        <a-input\r\n          v-model:value=\"dto.id\"\r\n          placeholder=\"输入客户证件号\"\r\n          size=\"large\"\r\n          style=\"width: 340px\"\r\n        ></a-input>\r\n      </a-col>\r\n      <a-col :span=\"1\"></a-col>\r\n\r\n      <a-col :span=\"2\">\r\n        <a-button @click=\"searchbyname\" type=\"primary\" size=\"large\"\r\n          >搜索</a-button\r\n        >\r\n      </a-col>\r\n    </a-row>\r\n    <hr />\r\n\r\n    <!-- 表格 -->\r\n    <div class=\"table\">\r\n      <a-table\r\n        :columns=\"columns\"\r\n        :dataSource=\"bankaccount\"\r\n        rowKey=\"id\"\r\n        :pagination=\"pagination\"\r\n        :scroll=\"{ y: 450 }\"\r\n        @change=\"tableChange\"\r\n      >\r\n        <template #bodyCell=\"{ column, record }\">\r\n          <template v-if=\"column.dataIndex === 'operation'\">\r\n            <a-popconfirm\r\n              placement=\"left\"\r\n              title=\"确认要修改该条信息吗?\"\r\n              ok-text=\"确定\"\r\n              cancel-text=\"取消\"\r\n              @confirm=\"showModal(record.account, record.balance, record.id)\"\r\n            >\r\n              <a>修改</a>\r\n            </a-popconfirm>\r\n            <a-divider type=\"vertical\"></a-divider>\r\n\r\n            <a-popconfirm\r\n              placement=\"left\"\r\n              title=\"确认要删除该账户吗?\"\r\n              ok-text=\"确定\"\r\n              cancel-text=\"取消\"\r\n              @confirm=\"onDelete(record.account, record.id)\"\r\n            >\r\n              <a>销户</a>\r\n            </a-popconfirm>\r\n          </template>\r\n        </template>\r\n      </a-table>\r\n    </div>\r\n\r\n    <!-- 修改功能 -->\r\n    <div>\r\n      <a-modal\r\n        v-model:open=\"open\"\r\n        title=\"修改账户信息\"\r\n        ok-text=\"确定修改\"\r\n        cancel-text=\"取消\"\r\n        width=\"1200px\"\r\n        @ok=\"handleOk\"\r\n      >\r\n        <div>\r\n          <br />\r\n          <a-row>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <a-input\r\n                v-model:value=\"newdata.account\"\r\n                placeholder=\"银行卡号\"\r\n                size=\"large\"\r\n                :disabled=\"true\"\r\n              />\r\n            </a-col>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <a-input\r\n                v-model:value=\"newdata.id\"\r\n                size=\"large\"\r\n                placeholder=\"证件号\"\r\n              />\r\n            </a-col>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <a-input\r\n                v-model:value=\"newdata.balance\"\r\n                size=\"large\"\r\n                placeholder=\"账户余额\"\r\n              />\r\n            </a-col>\r\n          </a-row>\r\n\r\n          <br />\r\n          <br />\r\n\r\n          <a-row>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <a-input\r\n                v-model:value=\"newdata.bank\"\r\n                size=\"large\"\r\n                placeholder=\"银行名称\"\r\n              />\r\n            </a-col>\r\n            <a-col :span=\"1\"></a-col>\r\n            <a-col :span=\"6\">\r\n              <a-input\r\n                v-model:value=\"newdata.branch\"\r\n                size=\"large\"\r\n                placeholder=\"分行名称\"\r\n              />\r\n            </a-col>\r\n          </a-row>\r\n\r\n          <br />\r\n          <br />\r\n\r\n          <br />\r\n        </div>\r\n      </a-modal>\r\n    </div>\r\n\r\n    <!-- 新增功能 -->\r\n    <a-modal\r\n      v-model:open=\"open2\"\r\n      title=\"新增账户\"\r\n      ok-text=\"确定\"\r\n      cancel-text=\"取消\"\r\n      @ok=\"handleOk2\"\r\n    >\r\n      <br />\r\n      <!-- 选择客户 -->\r\n      <a-space>\r\n        <span>选择交易客户：</span>\r\n        <a-button @click=\"sumShow = true\">选择客户</a-button>\r\n      </a-space>\r\n      <br />\r\n      <br />\r\n\r\n      <a-row>\r\n        <a-col :span=\"1\"></a-col>\r\n        <a-col :span=\"6\">\r\n          <a-input\r\n            v-model:value=\"selectedCustomer.name\"\r\n            placeholder=\"姓名\"\r\n            size=\"large\"\r\n            :disabled=\"true\"\r\n          />\r\n        </a-col>\r\n        <a-col :span=\"1\"></a-col>\r\n        <a-col :span=\"6\">\r\n          <a-input\r\n            v-model:value=\"newaccount.id\"\r\n            placeholder=\"证件号\"\r\n            size=\"large\"\r\n            :disabled=\"true\"\r\n          />\r\n        </a-col>\r\n        <a-col :span=\"1\"></a-col>\r\n        <a-col :span=\"6\">\r\n          <a-input\r\n            v-model:value=\"newaccount.account\"\r\n            placeholder=\"银行卡号\"\r\n            size=\"large\"\r\n          />\r\n        </a-col>\r\n      </a-row>\r\n\r\n      <br />\r\n      <br />\r\n      <a-row>\r\n        <a-col :span=\"1\"></a-col>\r\n        <a-col :span=\"6\">\r\n          <a-input\r\n            v-model:value=\"newaccount.bank\"\r\n            size=\"large\"\r\n            placeholder=\"银行名称\"\r\n          />\r\n        </a-col>\r\n        <a-col :span=\"1\"></a-col>\r\n        <a-col :span=\"6\">\r\n          <a-input\r\n            v-model:value=\"newaccount.branch\"\r\n            size=\"large\"\r\n            placeholder=\"分行名称\"\r\n          />\r\n        </a-col>\r\n        <a-col :span=\"1\"></a-col>\r\n        <a-col :span=\"6\">\r\n          <a-input\r\n            v-model:value=\"newaccount.balance\"\r\n            size=\"large\"\r\n            placeholder=\"余额\"\r\n          />\r\n        </a-col>\r\n      </a-row>\r\n    </a-modal>\r\n  </div>\r\n\r\n  <!-- 选择客户对话框 -->\r\n  <SelectUserModal\r\n    v-model:show-modal=\"sumShow\"\r\n    :handle-ok=\"\r\n      (c) => {\r\n        selectedCustomer = c;\r\n        console.log(selectedCustomer.value);\r\n        newaccount.id = selectedCustomer.id;\r\n        sumShow = false;\r\n      }\r\n    \"\r\n    :handle-cancel=\"\r\n      () => {\r\n        sumShow = false;\r\n      }\r\n    \"\r\n  />\r\n</template>\r\n\r\n<script setup lang=\"js\">\r\n\r\nimport { ref, computed, onMounted} from 'vue';\r\nimport axios from \"../api/request\";\r\nimport { message } from 'ant-design-vue';\r\nimport SelectUserModal from \"@/components/Business/Tools/SelectUserModal.vue\";\r\n\r\nconst sumShow = ref(false);\r\n\r\nconst selectedCustomer = ref({\r\n        id: \"\",\r\n        name: \"\"\r\n      })\r\nconst dto = ref({ current: 1, size: 5,id:''})\r\nconst data = ref([]);\r\n\r\n\r\n\r\n\r\n// >>>>>>>>>数据获取\r\nasync function getdata() {\r\n  try {\r\n\r\n    const resp = await axios.get('/account/all', {\r\n      params: {\r\n        current: dto.value.current,\r\n        size: dto.value.size\r\n      }\r\n    })\r\n    //提出响应数据对象\r\n    data.value = resp.data;\r\n    // 处理数据\r\n    console.log('获取成功');\r\n    console.log(data.value);\r\n\r\n\r\n  } catch (error) {\r\n    // 处理错误\r\n    console.error(error);\r\n\r\n\r\n  }\r\n}\r\n\r\n// >>>>>>>>组件加载后第一次获取数据\r\nonMounted(() => {\r\n    getdata()\r\n})\r\n\r\nconst bankaccount = computed(()=>{\r\n  return data.value|| []\r\n})\r\n\r\n// >>>>>>>>用于分页\r\nconst pagination = computed(()=>{\r\n  return {\r\n    current: dto.value.current, // 当前页\r\n    pageSize: dto.value.size, // 页大小\r\n    total: bankaccount.length,       // 总记录数\r\n    showSizeChanger: true,    // 显示页大小的下拉列表\r\n    pageSizeOptions: [\"3\",\"5\",\"10\",\"50\",\"100\"] // 自定义下拉列表内容\r\n  }\r\n})\r\n// 在页号或页大小改变时调用\r\nfunction tableChange(pagination) {\r\n  console.log(pagination)\r\n  dto.value.current = pagination.current ?? 1\r\n  dto.value.size = pagination.pageSize ?? 5\r\n  // run(dto.value)\r\n}\r\n\r\n// >>>>>>>>>>>>>>>>>>>>>>>搜索功能开始\r\nasync function searchbyname() {\r\n  console.log(dto.value.id)\r\n  console.log(typeof (dto.value.id));\r\n    if (dto.value.id == '') {\r\n        await getdata();\r\n    }\r\n    else {\r\n        axios.get('/account',\r\n            {\r\n                params: {\r\n                    id: dto.value.id,\r\n                    current: dto.value.current,\r\n                    size: dto.value.size\r\n                }\r\n            })\r\n            .then(resp => {\r\n                //提出响应数据对象\r\n                data.value = resp.data;\r\n                // 处理数据\r\n                console.log('获取成功');\r\n                console.log(data.value);\r\n            })\r\n            .catch(error => {\r\n                // 处理错误\r\n                console.error(error);\r\n            });\r\n\r\n\r\n    }\r\n\r\n}\r\n\r\n// >>>>>>>>>>>>>>>>>>>>>>>>>搜索功结束\r\n\r\n\r\n// >>>>>>>>>>>>>> 销户功能开始\r\nfunction onDelete(myaccount,myid) {\r\n  console.log(myaccount);\r\n  console.log(myid);\r\n\r\n    axios.delete('/account', {\r\n            data: {\r\n                id: myid,\r\n                account: myaccount\r\n    } })\r\n  .then(response => {\r\n    console.log(response.data);\r\n\r\n      if (response.data.success) {\r\n          message.success('删除成功')\r\n          searchbyname()       //                   查询请求 查询响应\r\n\r\n      }\r\n    else\r\n      message.error('删除失败')\r\n\r\n\r\n  })\r\n  .catch(error => {\r\n    console.error(error);\r\n  });      // 删除请求 删除响应\r\n}\r\n\r\n// >>>>>>>>>>>>>> 删销户功能结束\r\n\r\n\r\n// >>>>>>>>>>>>>> 修改功能开始\r\n\r\nconst newdata = ref({\r\n    id: \"\",\r\n    account: \"\",\r\n    balance: \"\",\r\n    bank: \"\",\r\n    branch: \"\",\r\n\r\n})\r\n\r\nconst open = ref(false);\r\n\r\n//打开对话框\r\nconst showModal = (account,id) => {\r\n  open.value = true;\r\n  newdata.value.account =account;\r\n  newdata.value.id = id;\r\n};\r\n\r\n//确认事件\r\nasync function handleOk() {\r\n  await modifycostomer();\r\n  searchbyname();\r\n  console.log('newdata', newdata.value);\r\n\r\n  open.value = false;\r\n\r\n};\r\n\r\n//发送修改信息请求\r\nfunction modifycostomer() {\r\n    newdata.value.balance=parseFloat(newdata.value.balance)\r\n\r\n    axios.post('/account',newdata.value)\r\n        .then(response => {\r\n          const resp = response.data\r\n          console.log(resp)\r\n          if (resp.success) {\r\n            searchbyname();\r\n            message.success('修改账户信息成功', 5);\r\n            }\r\n            else {\r\n                message.info(`修改账户信息失败,${resp.message}`);\r\n        }\r\n\r\n        })\r\n        .catch(error => {\r\n          console.error(error);\r\n        });\r\n\r\n\r\n}\r\n\r\n\r\n// >>>>>>>>>>>>>> 修改功能结束\r\n\r\n\r\n\r\n\r\n\r\n// >>>>>>>>>>>>>新增功能开始\r\n\r\nconst open2 = ref(false);\r\n\r\nconst showModal2 = () => {\r\n  open2.value = true;\r\n};\r\n\r\nasync function handleOk2() {\r\n    //   console.log(e);\r\n    await add();\r\n    open2.value = false;\r\n}\r\n\r\n//新增账户信息\r\nconst newaccount = ref({\r\n    id: \"\",\r\n    account: \"\",\r\n    balance: \"\",\r\n    bank: \"\",\r\n    branch: \"\",\r\n});\r\n\r\n\r\n//新增账户函数\r\nfunction add() {\r\n    newaccount.value.balance = parseFloat(newaccount.value.balance)\r\n    console.log(newaccount.value)\r\n    axios.put('/account', newaccount.value)\r\n        .then(response => {\r\n            const resp = response.data\r\n            console.log(resp)\r\n            if (resp.success) {\r\n                message.success('添加账户成功', 5);\r\n                searchbyname();\r\n            }\r\n            else {\r\n                message.error(`添加账户失败,${resp.message}`);\r\n            }\r\n\r\n        })\r\n        .catch(error => {\r\n            console.error(error);\r\n        });\r\n\r\n\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n// >>>>>>>>>>>>新增功能结束\r\n\r\n// >>>>>>>>>>>>表格列\r\nconst columns = ref([\r\n  {\r\n    title:'卡号',\r\n    dataIndex: 'account',\r\n    key : 'account'\r\n    },\r\n\r\n  {\r\n    title:'客户证件号或代码',\r\n    dataIndex: 'id',\r\n    key : 'id'\r\n    },\r\n\r\n  {\r\n    title:'账户余额',\r\n    dataIndex: 'balance',\r\n    key : '1'\r\n    },\r\n    {\r\n    title:'银行名称',\r\n    dataIndex: 'bank',\r\n    key : '0'\r\n  },\r\n  {\r\n    title:'分行名称',\r\n    dataIndex: 'branch',\r\n    key : '2'\r\n\r\n  },\r\n  {\r\n    title: '操作',\r\n    dataIndex: 'operation',\r\n    key: '4',\r\n    fixed: 'right',\r\n    // width: 100,\r\n\r\n  }\r\n])\r\n</script>\r\n\r\n<style scoped>\r\n.table {\r\n  width: 90%;\r\n}\r\n</style>\r\n"],"mappings":"AA4OA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,SAAS,QAAO,KAAK;AAC7C,OAAOC,KAAK,MAAM,gBAAgB;AAClC,SAASC,OAAO,QAAQ,gBAAgB;AACxC,OAAOC,eAAe,MAAM,iDAAiD;;;;;;;IAE7E,MAAMC,OAAO,GAAGN,GAAG,