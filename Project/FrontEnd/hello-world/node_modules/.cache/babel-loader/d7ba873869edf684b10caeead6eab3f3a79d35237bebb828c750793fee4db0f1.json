\n}\r\n\r\n.form > .btn {\r\n  margin-top: 1.5rem;\r\n}\r\n\r\n.btn:active {\r\n  transform: scale(0.95);\r\n}\r\n\r\n.btn:focus {\r\n  outline: none;\r\n}\r\n\r\n.form {\r\n  background-color: #e9e9e9;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  flex-direction: column;\r\n  padding: 0 3rem;\r\n  height: 100%;\r\n  text-align: center;\r\n}\r\n\r\n.input {\r\n  background-color: #fff;\r\n  border: none;\r\n  padding: 0.9rem 0.9rem;\r\n  margin: 0.5rem 0;\r\n  width: 100%;\r\n}\r\n\r\n@keyframes show {\r\n  0%,\r\n  49.99% {\r\n    opacity: 0;\r\n    z-index: 1;\r\n  }\r\n\r\n  50%,\r\n  100% {\r\n    opacity: 1;\r\n    z-index: 5;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";AAgGA,SAASA,OAAO,QAAQ,gBAAgB;AACxC,SAASC,GAAG,QAAQ,KAAK;AACzB,OAAOC,KAAK,MAAM,gBAAgB;AAClC,SAASC,SAAS,QAAQ,YAAY;;AAItC;;;;;;;;IACA,MAAMC,kBAAkB,GAAGH,GAAG,CAAC,KAAK,CAAC;IACrC,MAAMI,iBAAiB,GAAGA,CAAA,KAAM;MAC9BD,kBAAkB,CAACE,KAAK,GAAG,KAAK;MAChC;IACF,CAAC;IACD,MAAMC,kBAAkB,GAAGA,CAAA,KAAM;MAC/BH,kBAAkB,CAACE,KAAK,GAAG,IAAI;MAC/B;IAEF,CAAC;;IAED;IACE;IACF,MAAME,MAAM,GAAGP,GAAG,CAAC;MACjBQ,EAAE,EAAE,EAAE;MACNC,QAAQ,EAAE;IACZ,CAAC,CAAC;IACF,MAAMC,MAAM,GAAGV,GAAG,CAAC;MACjBQ,EAAE,EAAE,EAAE;MACNC,QAAQ,EAAE,EAAE;MACZE,QAAQ,EAAE;IACZ,CAAC,CAAC;;IAEF;IACA,MAAMC,MAAM,GAAGV,SAAS,CAAC,CAAC;;IAExB;IACF,eAAeW,OAAOA,CAAA,EAAG;MACvB,IAAI;QAEFH,MAAM,CAACL,KAAK,CAACG,EAAE,GAAGM,QAAQ,CAACJ,MAAM,CAACL,KAAK,CAACG,EAAE,EAAE,EAAE,CAAC;QAC/CO,OAAO,CAACC,GAAG,CAACN,MAAM,CAACL,KAAK,CAAC;QACzB,MAAMY,IAAI,GAAG,MAAMhB,KAAK,CAACiB,IAAI,CAAC,gBAAgB,EAAER,MAAM,CAACL,KAAK,CAAC;QAC7D;QACA,MAAMc,IAAI,GAAGF,IAAI,CAACE,IAAI;QACtB;QACAJ,OAAO,CAACC,GAAG,CAACG,IAAI,CAAC;QACjB,IAAIA,IAAI,CAACC,OAAO,EAAE;UAChBL,OAAO,CAACC,GAAG,CAAC,MAAM,CAAC;UACnBjB,OAAO,CAACqB,OAAO,CAAC,MAAM,EAAE,CAAC,CAAC;QAE5B;MACF,CAAC,CAAC,OAAOC,KAAK,EAAE;QACd;QACAN,OAAO,CAACM,KAAK,CAACA,KAAK,CAAC;MACtB;IAEF;IAEA,eAAeC,KAAKA,CAAA,EAAG;MACrB,IAAI;QACFP,OAAO,CAACC,GAAG,CAACT,MAAM,CAACF,KAAK,CAAC;QACzBkB,YAAY,CAACC,UAAU,CAAC,OAAO,CAA