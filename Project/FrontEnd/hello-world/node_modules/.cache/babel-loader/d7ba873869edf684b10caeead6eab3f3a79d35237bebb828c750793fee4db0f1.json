{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { message } from 'ant-design-vue';\nimport { ref } from 'vue';\nimport axios from \"../api/request\";\nimport { useRouter } from 'vue-router';\n\n//滑动效果js代码段\n\nexport default {\n  __name: 'log-in',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const isRightPanelActive = ref(false);\n    const activateLeftPanel = () => {\n      isRightPanelActive.value = false;\n      // console.log('left');\n    };\n    const activateRightPanel = () => {\n      isRightPanelActive.value = true;\n      // console.log('right');\n    };\n\n    //POST请求发送\n    //发送数据Object\n    const signIn = ref({\n      id: '',\n      password: ''\n    });\n    const signUp = ref({\n      id: '',\n      password: '',\n      username: ''\n    });\n\n    //控制跳转路径\n    const router = useRouter();\n\n    //发送请求\n    async function addUser() {\n      try {\n        signUp.value.id = parseInt(signUp.value.id, 10);\n        console.log(signUp.value);\n        const resp = await axios.post('/user/register', signUp.value);\n        //提出响应数据对象\n        const data = resp.data;\n        // 处理数据\n        console.log(data);\n        if (data.success) {\n          console.log('注册成功');\n          message.success('注册成功', 2);\n        }\n      } catch (error) {\n        // 处理错误\n        console.error(error);\n      }\n    }\n    async function logIn() {\n      try {\n        console.log(signIn.value);\n        localStorage.removeItem('token');\n        const resp = await axios.post('/user/login', signIn.value);\n        //提出响应数据对象\n        const data = resp.data;\n        // 处理数据\n        console.log(data);\n        if (data.success) {\n          console.log('登录成功');\n          message.success('登录成功', 2);\n          console.log(data.data);\n          localStorage.setItem('token', data.data);\n          router.push({\n            path: '/menu'\n          });\n        } else {\n          message.error('账号或密码错误');\n        }\n      } catch (error) {\n        // 处理错误\n        console.error(error);\n        router.push('/404');\n      }\n    }\n    const __returned__ = {\n      isRightPanelActive,\n      activateLeftPanel,\n      activateRightPanel,\n      signIn,\n      signUp,\n      router,\n      addUser,\n      logIn,\n      get message() {\n        return message;\n      },\n      ref,\n      get axios() {\n        return axios;\n      },\n      get useRouter() {\n        return useRouter;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["message","ref","axios","useRouter","isRightPanelActive","activateLeftPanel","value","activateRightPanel","signIn","id","password","signUp","username","router","addUser","parseInt","console","log","resp","post","data","success","error","logIn","localStorage","removeItem","setItem","push","path"],"sources":["H:/GitHub-Repository/Java-2023/Project/FrontEnd/hello-world/src/components/log-in.vue"],"sourcesContent":["<template>\r\n  <div class=\"body\">\r\n    <div class=\"container\">\r\n      <div :class=\"{ 'right-panel-active': isRightPanelActive }\">\r\n        <!-- Sign Up -->\r\n        <div class=\"container__form container--signup\">\r\n          <form action=\"#\" class=\"form\" id=\"form1\" @submit.prevent>\r\n            <h2 class=\"form__title\">Sign Up</h2>\r\n            <input\r\n              type=\"text\"\r\n              placeholder=\"Username\"\r\n              class=\"input\"\r\n              v-model=\"signUp.username\"\r\n              autocomplete=\"off\"\r\n            />\r\n            <input\r\n              type=\"text\"\r\n              placeholder=\"Id\"\r\n              class=\"input\"\r\n              v-model=\"signUp.id\"\r\n              autocomplete=\"off\"\r\n            />\r\n            <input\r\n              type=\"password\"\r\n              placeholder=\"Password\"\r\n              class=\"input\"\r\n              v-model=\"signUp.password\"\r\n              autocomplete=\"off\"\r\n            />\r\n\r\n            <button class=\"btn\" @click=\"addUser\">Sign Up</button>\r\n          </form>\r\n        </div>\r\n\r\n        <!-- Sign In -->\r\n\r\n        <div class=\"container__form container--signin\">\r\n          <div class=\"form\" id=\"form2\">\r\n            <h2 class=\"form__title\">Sign In</h2>\r\n            <input\r\n              type=\"text\"\r\n              placeholder=\"Id\"\r\n              class=\"input\"\r\n              v-model=\"signIn.id\"\r\n              autocomplete=\"off\"\r\n            />\r\n            <input\r\n              type=\"password\"\r\n              placeholder=\"Password\"\r\n              class=\"input\"\r\n              v-model=\"signIn.password\"\r\n              autocomplete=\"off\"\r\n            />\r\n            <button class=\"btn\" @click=\"logIn\">Sign In</button>\r\n          </div>\r\n          <!-- <form action=\"#\" class=\"form\" id=\"form2\" @submit.prevent>\r\n            <h2 class=\"form__title\">Sign In</h2>\r\n            <input\r\n              type=\"email\"\r\n              placeholder=\"Email\"\r\n              class=\"input\"\r\n              v-model=\"signIn.email\"\r\n              autocomplete=\"off\"\r\n            />\r\n            <input\r\n              type=\"password\"\r\n              placeholder=\"Password\"\r\n              class=\"input\"\r\n              v-model=\"signIn.password\"\r\n              autocomplete=\"off\"\r\n            />\r\n            <button class=\"btn\" @click=\"logIn\">Sign In</button>\r\n          </form> -->\r\n        </div>\r\n\r\n        <!-- Overlay -->\r\n        <div class=\"container__overlay\">\r\n          <div class=\"overlay\">\r\n            <div class=\"overlay__panel overlay--left\">\r\n              <button class=\"btn\" id=\"signIn\" @click=\"activateLeftPanel\">\r\n                Sign In\r\n              </button>\r\n            </div>\r\n            <div class=\"overlay__panel overlay--right\">\r\n              <button class=\"btn\" id=\"signUp\" @click=\"activateRightPanel\">\r\n                Sign Up\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup lang=\"js\">\r\nimport { message } from 'ant-design-vue';\r\nimport { ref } from 'vue';\r\nimport axios from \"../api/request\";\r\nimport { useRouter } from 'vue-router';\r\n\r\n\r\n\r\n//滑动效果js代码段\r\nconst isRightPanelActive = ref(false);\r\nconst activateLeftPanel = () => {\r\n  isRightPanelActive.value = false;\r\n  // console.log('left');\r\n};\r\nconst activateRightPanel = () => {\r\n  isRightPanelActive.value = true;\r\n  // console.log('right');\r\n\r\n};\r\n\r\n//POST请求发送\r\n  //发送数据Object\r\nconst signIn = ref({\r\n  id: '',\r\n  password: '',\r\n})\r\nconst signUp = ref({\r\n  id: '',\r\n  password: '',\r\n  username: '',\r\n})\r\n\r\n//控制跳转路径\r\nconst router = useRouter();\r\n\r\n  //发送请求\r\nasync function addUser() {\r\n  try {\r\n\r\n    signUp.value.id = parseInt(signUp.value.id, 10);\r\n    console.log(signUp.value)\r\n    const resp = await axios.post('/user/register', signUp.value)\r\n    //提出响应数据对象\r\n    const data = resp.data;\r\n    // 处理数据\r\n    console.log(data);\r\n    if (data.success) {\r\n      console.log('注册成功')\r\n      message.success('注册成功', 2);\r\n\r\n    }\r\n  } catch (error) {\r\n    // 处理错误\r\n    console.error(error);\r\n  }\r\n\r\n}\r\n\r\nasync function logIn() {\r\n  try {\r\n    console.log(signIn.value)\r\n    localStorage.removeItem('token');\r\n\r\n    const resp = await axios.post('/user/login',signIn.value)\r\n    //提出响应数据对象\r\n    const data = resp.data;\r\n    // 处理数据\r\n    console.log(data);\r\n    if (data.success) {\r\n      console.log('登录成功')\r\n      message.success('登录成功', 2);\r\n\r\n      console.log(data.data)\r\n\r\n      localStorage.setItem('token', data.data);\r\n      router.push({ path: '/menu' })\r\n    }else\r\n    {\r\n      message.error('账号或密码错误');\r\n    }\r\n\r\n\r\n\r\n  } catch (error) {\r\n    // 处理错误\r\n    console.error(error);\r\n    router.push('/404')\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n:root {\r\n  /* COLORS */\r\n  --white: #e9e9e9;\r\n  --gray: #333;\r\n  --blue: #0367a6;\r\n  --lightblue: #008997;\r\n\r\n  /* RADII */\r\n  --button-radius: 0.7rem;\r\n\r\n  /* SIZES */\r\n  --max-width: 758px;\r\n  --max-height: 420px;\r\n\r\n  font-size: 16px;\r\n  font-family: -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, Oxygen,\r\n    Ubuntu, Cantarell, \"Open Sans\", \"Helvetica Neue\", sans-serif;\r\n}\r\n\r\n.body {\r\n  align-items: center;\r\n  background-color: var(--white);\r\n  background: url(\"../assets/background_oeuhe7.jpg\");\r\n  /* 决定背景图像的位置是在视口内固定，或者随着包含它的区块滚动。 */\r\n  /* https://developer.mozilla.org/zh-CN/docs/Web/CSS/background-attachment */\r\n  background-attachment: fixed;\r\n  background-position: center;\r\n  background-repeat: no-repeat;\r\n  background-size: cover;\r\n  display: grid;\r\n  height: 100vh;\r\n  place-items: center;\r\n}\r\n.form__title {\r\n  font-weight: 300;\r\n  margin: 0;\r\n  margin-bottom: 1.25rem;\r\n}\r\n\r\n.link {\r\n  color: #333;\r\n  font-size: 0.9rem;\r\n  margin: 1.5rem 0;\r\n  text-decoration: none;\r\n}\r\n\r\n.container {\r\n  background-color: #e9e9e9;\r\n  border-radius: 0.7rem;\r\n  box-shadow: 0 0.9rem 1.7rem rgba(0, 0, 0, 0.25),\r\n    0 0.7rem 0.7rem rgba(0, 0, 0, 0.22);\r\n  height: 420px;\r\n  max-width: 758px;\r\n  overflow: hidden;\r\n  position: relative;\r\n  width: 100%;\r\n}\r\n\r\n.container__form {\r\n  height: 100%;\r\n  position: absolute;\r\n  top: 0;\r\n  transition: all 0.6s ease-in-out;\r\n}\r\n\r\n.container--signin {\r\n  left: 0;\r\n  width: 50%;\r\n  z-index: 2;\r\n}\r\n\r\n.right-panel-active .container--signin {\r\n  transform: translateX(100%);\r\n}\r\n\r\n.container--signup {\r\n  l