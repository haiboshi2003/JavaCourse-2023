{"ast":null,"code":"import axios from \"@/api/request\";\nimport { message } from \"ant-design-vue\";\nimport { ref, onMounted } from \"vue\";\nexport default {\n  __name: 'dialogTool',\n  props: [\"id\", \"dialogShow\"],\n  emits: [\"ifShow\"],\n  setup(__props, {\n    expose: __expose,\n    emit: __emit\n  }) {\n    __expose();\n    const props = __props;\n    const emits = __emit;\n    const title = ref({\n      id: \"\",\n      titleName: \"\",\n      projectName: \"\",\n      description: \"\",\n      textData: \"\",\n      imgPath: \"\",\n      svgPath: \"\",\n      voicePath: \"\"\n    });\n    const handelConfirm = () => {\n      emits(\"ifShow\", false);\n    };\n    const handleExport = () => {\n      emits(\"ifShow\", false);\n    };\n    onMounted(() => {\n      axios.get(`/title/getById/${props.id}`).then(response => {\n        console.log(response.data);\n        const data = response.data.data;\n        if (response.data.code == 200) {\n          title.value.id = data.id;\n          title.value.projectName = data.projectName;\n          title.value.titleName = data.titleName;\n          title.value.description = data.description;\n          title.value.textData = data.textData;\n          title.value.imgPath = data.imgPath;\n          title.value.svgPath = data.svgPath;\n          title.value.voicePath = data.voicePath;\n        } else {\n          emits(\"ifShow\", false);\n        }\n      }).catch(error => {\n        console.error(\"Error fetching titles:\", error);\n      });\n    });\n    const __returned__ = {\n      props,\n      emits,\n      title,\n      handelConfirm,\n      handleExport,\n      get axios() {\n        return axios;\n      },\n      get message() {\n        return message;\n      },\n      ref,\n      onMounted\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["axios","message","ref","onMo